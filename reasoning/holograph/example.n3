# French roads
:paris :oneway :orleans.
:paris :oneway :chartres.
:paris :oneway :amiens.
:orleans :oneway :blois.
:orleans :oneway :bourges.
:blois :oneway :tours.
:chartres :oneway :lemans.
:lemans :oneway :angers.
:lemans :oneway :tours.
:angers :oneway :nantes.

# Horn Logic
{?A :oneway ?B} => {?A :path ?B}.
{?A :path ?B. ?B :path ?C} => {?A :path ?C}.

# query
{?A :path :nantes} =>> {?A :path :nantes}.
