@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix : <http://example.org/#>.

# slide 33 example from https://www.slideshare.net/PatHayes/blogic-iswc-2009-invited-talk

# owl restriction
:aaa owl:onProperty :bbb.
:aaa owl:allValuesFrom :ccc.

# the following 2 triples should entail :yyy a :ccc.
#:xxx a :aaa.
#:xxx :bbb :yyy.

# the following codex should entail :xxx a :aaa.
(_:y) log:npn (_:bng_1 _:bng_2 1).

_:bng_1 {
    :xxx :bbb _:y.
}

_:bng_2 {
    _:y a :ccc.
}

# owl:allValuseFrom description logic
(_:a skolem:b _:c) log:npn (_:bng_3 _:bng_4 1).

_:bng_3 {
    _:a owl:onProperty skolem:b.
    _:a owl:allValuesFrom _:c.
}

_:bng_4 {
    (_:x _:y) log:npn (_:bng_5 false 1).
    (_:x) log:npn (_:bng_7 false 1).
}

_:bng_5 {
    _:x a _:a.
    _:x skolem:b _:y.
    () log:npn (_:bng_6 false 1).
}

_:bng_6 {
    _:y a _:c.
}

_:bng_7 {
    (_:y) log:npn (_:bng_8 false 1).
    () log:npn (_:bng_10 false 1).
}

_:bng_8 {
    _:x skolem:b _:y.
    () log:npn (_:bng_9 false 1).
}

_:bng_9 {
    _:y a _:c.
}

_:bng_10 {
    _:x a _:a.
}

# query
(_:s _:o) log:npn (_:bng_11 _:bng_11 0).

_:bng_11 {
    _:s a _:o.
}
