@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/#>.

# beetle is a car
:beetle a :Car.

# all cars are green or blue
(_:A) log:npn (_:bng_1 _:bng_2 1).

_:bng_1 {
    _:A a :Car.
    () log:npn (_:bng_3 false 1).
}

_:bng_2 {
    _:A :is :green.
}

_:bng_3 {
    _:A :is :blue.
}

# green things are nice or pretty
(_:A) log:npn (_:bng_4 _:bng_5 1).

_:bng_4 {
    _:A :is :green.
    () log:npn (_:bng_6 false 1).
}

_:bng_5 {
    _:A :is :nice.
}

_:bng_6 {
    _:A :is :pretty.
}

# pretty things are beautiful
(_:A) log:npn (_:bng_7 _:bng_8 1).

_:bng_7 {
    _:A :is :pretty.
}

_:bng_8 {
    _:A :is :beautiful.
}

# cars are not beautiful
(_:A) log:npn (_:bng_9 false 1).

_:bng_9 {
    _:A a :Car.
    _:A :is :beautiful.
}

# blue things are beautiful
(_:A) log:npn (_:bng_10 _:bng_11 1).

_:bng_10 {
    _:A :is :blue.
}

_:bng_11 {
    _:A :is :beautiful.
}

# query
(_:S _:O) log:npn (_:bng_12 _:bng_12 0).

_:bng_12 {
    _:S :is _:O.
}
