# --------------
# Beetle example
# --------------
#
# See https://en.wikipedia.org/wiki/Disjunction_elimination

@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <#>.

# beetle is a car
:beetle a :Car.

# all cars are green or blue
(_:A) log:nand _:bng_1.

_:bng_1 {
    _:A a :Car.
    () log:nand _:bng_2.
    () log:nand _:bng_3.
}

_:bng_2 {
    _:A :is :green.
}

_:bng_3 {
    _:A :is :blue.
}

# green things are beautiful
(_:A) log:nand _:bng_4.

_:bng_4 {
    _:A :is :green.
    () log:nand _:bng_5.
}

_:bng_5 {
    _:A :is :beautiful.
}

# blue things are beautiful
(_:A) log:nand _:bng_6.

_:bng_6 {
    _:A :is :blue.
    () log:nand _:bng_7
}

_:bng_7 {
    _:A :is :beautiful.
}

# query
(_:S _:O) log:nand _:bng_8.

_:bng_8 {
    _:S :is _:O.
    () log:nans _:bng_9.
}

_:bng_9 {
    _:S :is _:O.
}
