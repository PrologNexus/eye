#!/bin/bash
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope http://josd.github.io/eye/reasoning/socrates-metachain-b2f/metachain.n3 http://josd.github.io/eye/reasoning/socrates-metachain-b2f/socrates.n3 --query http://josd.github.io/eye/reasoning/socrates-metachain-b2f/socrates-query.n3 --output socrates-metachain-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. http://josd.github.io/eye/reasoning/socrates-metachain-b2f/metachain.n3 http://josd.github.io/eye/reasoning/socrates-metachain-b2f/socrates.n3 --query http://josd.github.io/eye/reasoning/socrates-metachain-b2f/socrates-query.n3 --output socrates-metachain-proof.n3
