@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        :x :saying :C.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :x :saying :C.
    };
    r:evidence (
        skolem:lemma2
        skolem:lemma3
        [ a r:Fact; r:gives {{
            :x :saying :A.
        } => {
            :x :saying :C.
        }}]
        [ a r:Fact; r:gives {{
            :x :saying :B.
        } => {
            :x :saying :C.
        }}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_3"]; r:boundTo {
        :x :saying :C.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo {
        :x a :Person.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo {
        :x :saying :A.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_2"]; r:boundTo {
        :x :saying :B.
    }];
    r:rule skolem:lemma4.

skolem:lemma2 a r:Inference;
    r:gives {
        {
            :x a :Person.
        } log:impliesOneOf ({
            :x :saying :A.
        } {
            :x :saying :B.
        }).
    };
    r:evidence (
        [ a r:Fact; r:gives true]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#x"]];
    r:rule skolem:lemma5.

skolem:lemma3 a r:Inference;
    r:gives {
        {
            :x a :Person.
        } log:call true.
    };
    r:evidence (
        [ a r:Fact; r:gives true]
    );
    r:rule [ a r:Fact; r:gives {true => {
        {
            :x a :Person.
        } log:call true.
    }}].

skolem:lemma4 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1, var:x_2, var:x_3. {
            var:x_0 log:impliesOneOf (var:x_1 var:x_2).
            var:x_0 log:call true.
            var:x_1 => var:x_3.
            var:x_2 => var:x_3.
        } => var:x_3.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/proof-by-exhaustion/test.n3>].

skolem:lemma5 a r:Extraction;
    r:gives {
        @forSome var:x_0. {
            {
                var:x_0 a :Person.
            } log:impliesOneOf ({
                var:x_0 :saying :A.
            } {
                var:x_0 :saying :B.
            }).
        } <= true.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/proof-by-exhaustion/test.n3>].

