@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.

# all cars are red or green or blue
{
    ?A a :Car.
} => (
    {
        ?A :is :red.
    }
    {
        ?A :is :green.
    }
    {
        ?A :is :blue.
    }
).

# red things are beautiful
{
    ?A :is :red.
} => {
    ?A :is :beautiful.
}.

# green things are beautiful
{
    ?A :is :green.
} => {
    ?A :is :beautiful.
}.

# blue things are beautiful
{
    ?A :is :blue.
} => {
    ?A :is :beautiful.
}.

# everything is not beautiful would be inconsistent
# {?A :is :beautiful} => ().
