#!/bin/bash -x
eye "$@" --wcache http://eyereasoner.github.io/eye/reasoning .. --nope http://eyereasoner.github.io/eye/reasoning/ifb/components.n3 http://eyereasoner.github.io/eye/reasoning/ifb/tasks.n3 --query http://eyereasoner.github.io/eye/reasoning/ifb/notIn_query.n3 --output notIn_answer.n3
eye "$@" --wcache http://eyereasoner.github.io/eye/reasoning .. --nope http://eyereasoner.github.io/eye/reasoning/ifb/components.n3 http://eyereasoner.github.io/eye/reasoning/ifb/tasks.n3 --query http://eyereasoner.github.io/eye/reasoning/ifb/forAllIn_query.n3 --output forAllIn_answer.n3
eye "$@" --wcache http://eyereasoner.github.io/eye/reasoning .. http://eyereasoner.github.io/eye/reasoning/ifb/components.n3 http://eyereasoner.github.io/eye/reasoning/ifb/tasks.n3 --query http://eyereasoner.github.io/eye/reasoning/ifb/notIn_query.n3 --output notIn_proof.n3
eye "$@" --wcache http://eyereasoner.github.io/eye/reasoning .. http://eyereasoner.github.io/eye/reasoning/ifb/components.n3 http://eyereasoner.github.io/eye/reasoning/ifb/tasks.n3 --query http://eyereasoner.github.io/eye/reasoning/ifb/forAllIn_query.n3 --output forAllIn_proof.n3
