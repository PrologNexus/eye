@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        (| "optimalTour" ("aaa" "bbb" "ccc" "ddd" "eee" "fff" "ggg") (("aaa" "bbb" "fff" "ccc" "eee" "ddd" "ggg") 75) |).
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        (| "optimalTour" ("aaa" "bbb" "ccc" "ddd" "eee" "fff" "ggg") (("aaa" "bbb" "fff" "ccc" "eee" "ddd" "ggg") 75) |).
    };
    r:evidence (
        [ a r:Fact; r:gives {(| "optimalTour" ("aaa" "bbb" "ccc" "ddd" "eee" "fff" "ggg") (("aaa" "bbb" "fff" "ccc" "eee" "ddd" "ggg") 75) |)}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo (("aaa" "bbb" "fff" "ccc" "eee" "ddd" "ggg") 75)];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0. (| "optimalTour" ("aaa" "bbb" "ccc" "ddd" "eee" "fff" "ggg") var:x_0 |) => (| "optimalTour" ("aaa" "bbb" "ccc" "ddd" "eee" "fff" "ggg") var:x_0 |)}].

