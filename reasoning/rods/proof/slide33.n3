@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        :xxx a :aaa.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :xxx a :aaa.
    };
    r:evidence (
        skolem:lemma2
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#xxx"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ n3:uri "http://example.org/#aaa"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0, var:x_1. skolem:bng_1 log:implies skolem:bng_2}].

skolem:lemma2 a r:Inference;
    r:gives {
        :xxx a :aaa.
    };
    r:evidence (
        skolem:lemma3
        skolem:lemma4
        [ a r:Fact; r:gives {(skolem:node_2) log:nand skolem:bng_3}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#aaa"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ n3:uri "http://example.org/#bbb"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_2"]; r:boundTo [ n3:uri "http://example.org/#ccc"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_3"]; r:boundTo [ n3:uri "http://example.org/#xxx"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0, var:x_1, var:x_2, var:x_3. skolem:bng_4 log:implies skolem:bng_5}].

skolem:lemma3 a r:Extraction;
    r:gives {
        :aaa owl:onProperty :bbb.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/rods/slide33.trig>].

skolem:lemma4 a r:Extraction;
    r:gives {
        :aaa owl:allValuesFrom :ccc.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/rods/slide33.trig>].


skolem:bng_8 {
    skolem:node_2 a var:x_2.
}

skolem:bng_1 {
    var:x_0 a var:x_1.
}

skolem:bng_2 {
    var:x_0 a var:x_1.
}

skolem:bng_3 {
    :xxx :bbb skolem:node_2.
    () log:nand skolem:bng_6.
}

skolem:bng_4 {
    var:x_0 owl:onProperty var:x_1.
    var:x_0 owl:allValuesFrom var:x_2.
    (skolem:node_2) log:nand skolem:bng_7.
}

skolem:bng_5 {
    var:x_3 a var:x_0.
}

skolem:bng_6 {
    skolem:node_2 a :ccc.
}

skolem:bng_7 {
    var:x_3 var:x_1 skolem:node_2.
    () log:nand skolem:bng_8.
}
