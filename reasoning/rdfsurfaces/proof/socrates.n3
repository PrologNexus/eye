@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://eyereasoner.github.io/eye/reasoning/rdfsurfaces/socrates.n3#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        :Socrates a :Mortal.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :Socrates a :Mortal.
    };
    r:evidence (
        skolem:lemma2
    );
    r:rule [ a r:Fact; r:gives {{
        :Socrates a :Mortal.
    } => {
        :Socrates a :Mortal.
    }}].

skolem:lemma2 a r:Inference;
    r:gives {
        :Socrates a :Mortal.
    };
    r:evidence (
        skolem:lemma3
        skolem:lemma4
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://eyereasoner.github.io/eye/reasoning/rdfsurfaces/socrates.n3#Human"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ n3:uri "http://eyereasoner.github.io/eye/reasoning/rdfsurfaces/socrates.n3#Mortal"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_2"]; r:boundTo [ n3:uri "http://eyereasoner.github.io/eye/reasoning/rdfsurfaces/socrates.n3#Socrates"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0, var:x_1, var:x_2. {
        var:x_0 rdfs:subClassOf var:x_1.
        var:x_2 a var:x_0.
    } => {
        var:x_2 a var:x_1.
    }}].

skolem:lemma3 a r:Extraction;
    r:gives {
        :Human rdfs:subClassOf :Mortal.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/rdfsurfaces/socrates.n3>].

skolem:lemma4 a r:Extraction;
    r:gives {
        :Socrates a :Human.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/rdfsurfaces/socrates.n3>].

