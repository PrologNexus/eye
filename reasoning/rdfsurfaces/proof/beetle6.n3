@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:component skolem:lemma2;
    r:gives {
        :beetle :is :green.
        :beetle :is :nice.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :beetle :is :green.
    };
    r:evidence (
        skolem:lemma3
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ n3:uri "http://example.org/#green"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0, var:x_1. {
        var:x_0 :is var:x_1.
    } => {
        var:x_0 :is var:x_1.
    }}].

skolem:lemma2 a r:Inference;
    r:gives {
        :beetle :is :nice.
    };
    r:evidence (
        skolem:lemma4
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ n3:uri "http://example.org/#nice"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0, var:x_1. {
        var:x_0 :is var:x_1.
    } => {
        var:x_0 :is var:x_1.
    }}].

skolem:lemma3 a r:Inference;
    r:gives {
        :beetle :is :green.
    };
    r:evidence (
        skolem:lemma5
        skolem:lemma6
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0. {
        var:x_0 a :Car.
        () log:onNegativeSurface {
            var:x_0 :is :blue.
        }.
    } => {
        var:x_0 :is :green.
    }}].

skolem:lemma4 a r:Inference;
    r:gives {
        :beetle :is :nice.
    };
    r:evidence (
        skolem:lemma3
        skolem:lemma7
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0. {
        var:x_0 :is :green.
        () log:onNegativeSurface {
            var:x_0 :is :pretty.
        }.
    } => {
        var:x_0 :is :nice.
    }}].

skolem:lemma5 a r:Extraction;
    r:gives {
        :beetle a :Car.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/rdfsurfaces/beetle6.n3s>].

skolem:lemma6 a r:Inference;
    r:gives {
        () log:onNegativeSurface {
            :beetle :is :blue.
        }.
    };
    r:evidence (
        skolem:lemma5
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0. {
        var:x_0 a :Car.
    } => {
        () log:onNegativeSurface {
            var:x_0 :is :blue.
        }.
    }}].

skolem:lemma7 a r:Inference;
    r:gives {
        () log:onNegativeSurface {
            :beetle :is :pretty.
        }.
    };
    r:evidence (
        skolem:lemma5
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0. {
        var:x_0 a :Car.
    } => {
        () log:onNegativeSurface {
            var:x_0 :is :pretty.
        }.
    }}].

