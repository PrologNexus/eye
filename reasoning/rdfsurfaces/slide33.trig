@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix : <http://example.org/#>.

# slide 33 example from https://www.slideshare.net/PatHayes/blogic-iswc-2009-invited-talk

# owl restriction
:aaa owl:onProperty :bbb.
:aaa owl:allValuesFrom :ccc.

# the following 2 triples should entail :yyy a :ccc.
#:xxx a :aaa.
#:xxx :bbb :yyy.

# the following codex should entail :xxx a :aaa.
(_:y) log:nand _:bng_1.

_:bng_1 {
    :xxx :bbb _:y.
    () log:nand _:bng_2.
}

_:bng_2 {
    _:y a :ccc.
}

# owl:allValuseFrom description logic
(_:a skolem:b _:c) log:nand _:bng_3.

_:bng_3 {
    _:a owl:onProperty skolem:b.
    _:a owl:allValuesFrom _:c.
    () log:nand _:bng_4.
}

_:bng_4 {
    (_:x _:y) log:nand _:bng_5.
    (_:x) log:nand _:bng_7.
}

_:bng_5 {
    _:x a _:a.
    _:x skolem:b _:y.
    () log:nand _:bng_6.
}

_:bng_6 {
    _:y a _:c.
}

_:bng_7 {
    (_:y) log:nand _:bng_8.
    () log:nand _:bng_10.
}

_:bng_8 {
    _:x skolem:b _:y.
    () log:nand _:bng_9.
}

_:bng_9 {
    _:y a _:c.
}

_:bng_10 {
    _:x a _:a.
}

# query
(_:x) log:nand _:bng_11.
_:bng_12 a log:Answer.

_:bng_11 {
    _:x a :aaa.
    () log:nand _:bng_12.
}

_:bng_12 {
    _:x a :aaa.
}
