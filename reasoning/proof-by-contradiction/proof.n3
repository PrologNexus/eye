@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        {
            :x :saying :C.
        } :provedBy :contradiction.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        {
            :x :saying :C.
        } :provedBy :contradiction.
    };
    r:evidence (
        skolem:lemma2
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo {
        :x :saying :C.
    }];
    r:rule skolem:lemma3.

skolem:lemma2 a r:Inference;
    r:gives {
        {
            {
                :x :saying :C.
            } => ($ $).
        } log:call {
            :x :saying :C.
        }.
    };
    r:evidence (
        skolem:lemma4
    );
    r:rule [ a r:Fact; r:gives {{
        :x :saying :C.
    } => {
        {
            {
                :x :saying :C.
            } => ($ $).
        } log:call {
            :x :saying :C.
        }.
    }}].

skolem:lemma3 a r:Extraction;
    r:gives {
        @forAll var:x_0. {
            {
                var:x_0 => ($ $).
            } log:call var:x_0.
        } => {
            var:x_0 :provedBy :contradiction.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/proof-by-contradiction/test.n3>].

skolem:lemma4 a r:Inference;
    r:gives {
        :x :saying :C.
    };
    r:evidence (
        skolem:lemma5
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#x"]];
    r:rule skolem:lemma6.

skolem:lemma5 a r:Extraction;
    r:gives {
        :x :saying :A.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/proof-by-contradiction/test.n3>].

skolem:lemma6 a r:Extraction;
    r:gives {
        @forAll var:x_0. {
            var:x_0 :saying :A.
        } => {
            var:x_0 :saying :C.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/proof-by-contradiction/test.n3>].

