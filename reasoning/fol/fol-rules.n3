# ---------
# FOL rules
# ---------

@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/#>.

# resolution
{
    ?A => {
        ?B :or ?C.
    }.
    ?B => ?D.
    ?C => ?E.
} => {
    ?A => {
        ?D :or ?E.
    }.
}.

# factoring
{
    ?A => {
        ?B :or ?B.
    }.
} => {
    ?A => ?B.
}.

# negation
{
    ?A => ?B.
    ?B :not true.
} => {
    ?A :not true.
}.

{
    ?A => {
        ?B :or ?C.
    }.
    ?B :not true.
} => {
    ?A => ?C.
}.

{
    ?A => {
        ?B :or ?C.
    }.
    ?C :not true.
} => {
    ?A => ?B.
}.

# blow a fuse
{
    ?A :not true.
    ?A log:call true.
} => false.
