# ---------------------
# Generating test facts
# ---------------------

@prefix e: <http://eulersharp.sourceforge.net/2003/03swap/log-rules#>.
@prefix : <https://eyereasoner.github.io/eye/reasoning#>.

# graph generation
{(?F ?N) :graphgen true} <= {
    ("open" ?F "write" ?FD) log:herbrandModel true.
    ("tell" ?FD) log:herbrandModel true.
    ("format" "@prefix : <http://eulersharp.sourceforge.net/2009/12dtb/test#>.~n~n" ()) log:herbrandModel true.
    ("between" 1 ?N ?I) log:herbrandModel true.
    ("format" ":i~d a :N0.~n" (?I)) log:herbrandModel true.
    ("fail") log:herbrandModel true.
}.
{(?F ?N) :graphgen true} <= {
    ("told") log:herbrandModel true.
}.

# immediate goal
true <= {
    ("test-facts.ttl" 100000) :graphgen true.
}.
