@prefix : <http://example.org/#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.

:pers001 :familyName "Garcia".
:pers001 :givenName "Alex".
:pers002 :familyName "Cardinale".
:pers002 :givenName "Claudia".

# explanation
(_:bng_1 log:implies true) log:explain ((var:person :pers001)).
(_:bng_2 log:query _:bng_2) log:explain ((var:person :pers001) (var:fname "Garcia") (var:gname "Alex")).
(_:bng_2 log:query _:bng_2) log:explain ((var:person :pers002) (var:fname "Cardinale") (var:gname "Claudia")).

_:bng_1 {
    var:person :familyName "Garcia".
    _:bng_3 log:becomes _:bng_4.
}

_:bng_2 {
    var:person :familyName var:fname.
    var:person :givenName var:gname.
}

_:bng_3 {
    var:person :givenName "Claudia".
}

_:bng_4 {
    var:person :givenName "Alex".
}
