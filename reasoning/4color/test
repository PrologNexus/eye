#!/bin/bash
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --tactic limited-answer 2 http://josd.github.io/eye/reasoning/4color/4color_data.n3 http://josd.github.io/eye/reasoning/4color/4color_rules.n3 --query http://josd.github.io/eye/reasoning/4color/4color_query.n3 --output 4color_answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --tactic limited-answer 2 http://josd.github.io/eye/reasoning/4color/4color_data.n3 http://josd.github.io/eye/reasoning/4color/4color_rules.n3 --query http://josd.github.io/eye/reasoning/4color/4color_query.n3 --output 4color_proof.n3
