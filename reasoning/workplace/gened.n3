# ------------------------
# Generating employee data
# ------------------------

@prefix e: <http://eulersharp.sourceforge.net/2003/03swap/log-rules#>.
@prefix : <https://eyereasoner.github.io/ns#>.

# workplace data generation
{(?F ?N) :gen true} <= {
    ("open" ?F "write" ?FD) log:herbrandModel true.
    ("tell" ?FD) log:herbrandModel true.
    ("format" "@prefix : <https://eyereasoner.github.io/ns#>.~n~n" ()) log:herbrandModel true.
    ("between" 1 ?N ?I) log:herbrandModel true.
    ("format" ":alice~d :does :log_off_at_end_of_shift.~n" (?I)) log:herbrandModel true.
    ("format" ":bob~d :does :work_related_task.~n" (?I)) log:herbrandModel true.
    ("format" ":bob~d :does :log_off_at_end_of_shift.~n" (?I)) log:herbrandModel true.
    ("format" ":carol~d :does :access_social_media.~n" (?I)) log:herbrandModel true.
    ("fail") log:herbrandModel true.
}.
{(?F ?N) :gen true} <= {
    ("told") log:herbrandModel true.
}.

# immediate goal
true <= {
    ("workplace-data.ttl" 5000) :gen true.
}.
