# ------------------
# Semifol resolution
# ------------------

@prefix log: <http://www.w3.org/2000/10/swap/log#>.

# resolution
{
    ?A => {
        ?B log:or ?C.
    }.
    ?B => ?D.
    ?C => ?E.
} => {
    ?A => {
        ?D log:or ?E.
    }.
}.

{
    ?A => ?B.
    ?B log:not true.
} => {
    true => {
        ?A log:not true.
    }.
}.

{
    ?A => {
        ?B log:or ?C.
    }.
    ?B log:not true.
} => {
    ?A => ?C.
}.

{
    ?A => {
        ?B log:or ?C.
    }.
    ?C log:not true.
} => {
    ?A => ?B.
}.

# factoring
{
    ?A => {
        ?B log:or ?B.
    }.
} => {
    ?A => ?B.
}.
