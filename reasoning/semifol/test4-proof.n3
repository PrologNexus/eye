@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:component skolem:lemma2;
    r:gives {
        {
            :Rex a :Bird.
        } log:not true.
        {
            :Rex a :Chicken.
        } log:not true.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        {
            :Rex a :Bird.
        } log:not true.
    };
    r:evidence (
        skolem:lemma3
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo {
        :Rex a :Bird.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo true];
    r:rule skolem:lemma4.

skolem:lemma2 a r:Inference;
    r:gives {
        {
            :Rex a :Chicken.
        } log:not true.
    };
    r:evidence (
        skolem:lemma5
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo {
        :Rex a :Chicken.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo true];
    r:rule skolem:lemma4.

skolem:lemma3 a r:Extraction;
    r:gives {
        {
            :Rex a :Bird.
        } log:not true.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/semifol/test4.n3>].

skolem:lemma4 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1. {
            var:x_0 log:not var:x_1.
        } => {
            var:x_0 log:not var:x_1.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/semifol/test4.n3>].

skolem:lemma5 a r:Inference;
    r:gives {
        {
            :Rex a :Chicken.
        } log:not true.
    };
    r:evidence (
        [ a r:Fact; r:gives true]
    );
    r:rule skolem:lemma6.

skolem:lemma6 a r:Inference;
    r:gives {
        true => {
            {
                :Rex a :Chicken.
            } log:not true.
        }.
    };
    r:evidence (
        [ a r:Fact; r:gives {{
            :Rex a :Chicken.
        } => {
            :Rex a :Bird.
        }}]
        skolem:lemma3
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo {
        :Rex a :Chicken.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo {
        :Rex a :Bird.
    }];
    r:rule skolem:lemma7.

skolem:lemma7 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1. {
            var:x_0 => var:x_1.
            var:x_1 log:not true.
        } => {
            true => {
                var:x_0 log:not true.
            }.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/semifol/semifol-resolution.n3>].

