@prefix : <http://josd.github.io/eye/reasoning#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix var: <http://josd.github.io/var#>.
@prefix skolem: <http://josd.github.io/.well-known/genid/3917894951695475063#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.

skolem:proof a r:Proof, r:Conjunction;
  r:component skolem:lemma1;
  r:gives {
    skolem:e_anon_1 :saying :C.
  }.

skolem:lemma1 a r:Inference;
  r:gives {
    skolem:e_anon_1 :saying :C.
  };
  r:evidence (
    skolem:lemma2
  );
  r:binding [ r:variable [ n3:uri "http://josd.github.io/var#x_0"]; r:boundTo [ a r:Existential; n3:nodeId "http://josd.github.io/.well-known/genid/3917894951695475063#e_anon_1"]];
  r:rule skolem:lemma3.

skolem:lemma2 a r:Inference;
  r:gives {
    skolem:e_anon_1 :saying :C.
  };
  r:evidence (
    skolem:lemma4
  );
  r:binding [ r:variable [ n3:uri "http://josd.github.io/var#x_0"]; r:boundTo [ a r:Existential; n3:nodeId "http://josd.github.io/.well-known/genid/3917894951695475063#e_anon_1"]];
  r:rule skolem:lemma5.

skolem:lemma3 a r:Extraction;
  r:gives {
    @forAll var:x_0. {var:x_0 :saying :C} => {var:x_0 :saying :C}.
  };
  r:because [ a r:Parsing; r:source <http://josd.github.io/eye/reasoning/de/deQ.n3>].

skolem:lemma4 a r:Inference;
  r:gives {
    skolem:e_anon_1 :saying :A.
  };
  r:evidence (
    skolem:lemma6
  );
  r:binding [ r:variable [ n3:uri "http://josd.github.io/var#x_0"]; r:boundTo [ a r:Existential; n3:nodeId "http://josd.github.io/.well-known/genid/3917894951695475063#e_anon_1"]];
  r:rule skolem:lemma7.

skolem:lemma5 a r:Extraction;
  r:gives {
    @forAll var:x_0. {var:x_0 :saying :A} => {var:x_0 :saying :C}.
  };
  r:because [ a r:Parsing; r:source <http://josd.github.io/eye/reasoning/de/de.n3>].

skolem:lemma6 a r:Inference;
  r:gives {
    skolem:e_anon_1 :not_saying :B.
  };
  r:evidence (
    skolem:lemma8
  );
  r:binding [ r:variable [ n3:uri "http://josd.github.io/var#x_0"]; r:boundTo [ a r:Existential; n3:nodeId "http://josd.github.io/.well-known/genid/3917894951695475063#e_anon_1"]];
  r:rule skolem:lemma9.

skolem:lemma7 a r:Extraction;
  r:gives {
    @forAll var:x_0. {var:x_0 :not_saying :B} => {var:x_0 :saying :A}.
  };
  r:because [ a r:Parsing; r:source <http://josd.github.io/eye/reasoning/de/de.n3>].

skolem:lemma8 a r:Extraction;
  r:gives {
    skolem:e_anon_1 :not_saying :C.
  };
  r:because [ a r:Parsing; r:source <http://josd.github.io/eye/reasoning/de/deA.n3>].

skolem:lemma9 a r:Extraction;
  r:gives {
    @forAll var:x_0. {var:x_0 :not_saying :C} => {var:x_0 :not_saying :B}.
  };
  r:because [ a r:Parsing; r:source <http://josd.github.io/eye/reasoning/de/de.n3>].

