@prefix graph: <http://www.w3.org/2000/10/swap/graph#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/#>.

# blank node graphs
_:bng_1 {
    :s :p _:x.
}

_:bng_2 {
    :a :b :c.
    :d :e _:x.
}

# additional quad
:s :p :o _:bng_1.

# derive trig graph
{
    ?G graph:content ?C.
    ?C graph:member {
        :a :b :c.
    }.
} => {
    ?H graph:content {
        :u :v :w.
        :x :y :z.
    }.
}.

# query
{
    ?G graph:content ?C.
} =^ {
    ?G graph:content ?C.
}.
