@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        :test :is true.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :test :is true.
    };
    r:evidence (
        skolem:lemma2
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#test"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo true];
    r:rule skolem:lemma3.

skolem:lemma2 a r:Inference;
    r:gives {
        :test :is true.
    };
    r:evidence (
        [ a r:Fact; r:gives true]
    );
    r:rule skolem:lemma4.

skolem:lemma3 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1. {
            var:x_0 :is var:x_1.
        } => {
            var:x_0 :is var:x_1.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/inf/test3.n3>].

skolem:lemma4 a r:Inference;
    r:gives {
        true => {
            :test :is true.
        }.
    };
    r:evidence (
        skolem:lemma5
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo true];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo {
        :test :is true.
    }];
    r:rule skolem:lemma6.

skolem:lemma5 a r:Inference;
    r:gives {
        true => {
            {
                :test :is true.
            } log:or {
                :test :is true.
            }.
        }.
    };
    r:evidence (
        skolem:lemma7
        [ a r:Fact; r:gives {{
            :abc a :C3.
        } => {
            :test :is true.
        }}]
        [ a r:Fact; r:gives {{
            :abc a :C2.
        } => {
            :test :is true.
        }}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo true];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo {
        :abc a :C3.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_2"]; r:boundTo {
        :abc a :C2.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_3"]; r:boundTo {
        :test :is true.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_4"]; r:boundTo {
        :test :is true.
    }];
    r:rule skolem:lemma8.

skolem:lemma6 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1. {
            var:x_0 => {
                var:x_1 log:or var:x_1.
            }.
        } => {
            var:x_0 => var:x_1.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/inf/inf.n3>].

skolem:lemma7 a r:Inference;
    r:gives {
        true => {
            {
                :abc a :C3.
            } log:or {
                :abc a :C2.
            }.
        }.
    };
    r:evidence (
        skolem:lemma9
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo {
        :abc a :C1.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo {
        :abc a :C2.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_2"]; r:boundTo {
        :abc a :C3.
    }];
    r:rule skolem:lemma10.

skolem:lemma8 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1, var:x_2, var:x_3, var:x_4. {
            var:x_0 => {
                var:x_1 log:or var:x_2.
            }.
            var:x_1 => var:x_3.
            var:x_2 => var:x_4.
        } => {
            var:x_0 => {
                var:x_3 log:or var:x_4.
            }.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/inf/inf.n3>].

skolem:lemma9 a r:Inference;
    r:gives {
        {
            :abc a :C1.
        } log:or {
            {
                :abc a :C2.
            } log:or {
                :abc a :C3.
            }.
        }.
    };
    r:evidence (
        skolem:lemma11
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#abc"]];
    r:rule skolem:lemma12.

skolem:lemma10 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1, var:x_2. {
            var:x_0 log:or {
                var:x_1 log:or var:x_2.
            }.
        } => {
            true => {
                var:x_2 log:or var:x_1.
            }.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/inf/inf.n3>].

skolem:lemma11 a r:Extraction;
    r:gives {
        :abc a :C0.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/inf/test3.n3>].

skolem:lemma12 a r:Extraction;
    r:gives {
        @forAll var:x_0. {
            var:x_0 a :C0.
        } => {
            {
                var:x_0 a :C1.
            } log:or {
                {
                    var:x_0 a :C2.
                } log:or {
                    var:x_0 a :C3.
                }.
            }.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/inf/test3.n3>].

