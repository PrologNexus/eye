    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_4"]; r:boundTo true];
    r:rule skolem:lemma11.

skolem:lemma9 a r:Inference;
    r:gives {
        {
            {
                :i a :D.
            } => ().
            :i a :A.
        } => ({
            :i a :B.
        }).
    };
    r:evidence (
        skolem:lemma12
        [ a r:Fact; r:gives {({
            :i a :B.
        } {
            :i a :D.
        }) list:select ({
            :i a :D.
        } ({
            :i a :B.
        }))}]
        [ a r:Fact; r:gives {({
            {
                :i a :D.
            } => ().
        } {
            :i a :A.
        }) graph:union {
            {
                :i a :D.
            } => ().
            :i a :A.
        }}]
