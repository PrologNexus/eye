skolem:lemma13 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1, var:x_2, var:x_3, var:x_4. {
            var:x_0 => var:x_1.
            var:x_1 list:select (var:x_2 var:x_3).
