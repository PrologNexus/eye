skolem:lemma10 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1, var:x_2. {
            var:x_0 => var:x_1.
            var:x_1 list:removeDuplicates (var:x_2).
        } => {
            var:x_0 => var:x_2.
        }.
    };
    r:because [ a r:Parsing; r:source <file:///home/jdroo/github.com/eyereasoner/eye/reasoning/inf/inf.n3>].

skolem:lemma11 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1, var:x_2, var:x_3, var:x_4. {
            var:x_0 => ().
            var:x_0 graph:list var:x_1.
            var:x_1 list:select ({
                var:x_2 => ().
            } var:x_3).
            var:x_4 graph:list var:x_3.
        } => {
            var:x_4 => var:x_2.
        }.
    };
    r:because [ a r:Parsing; r:source <file:///home/jdroo/github.com/eyereasoner/eye/reasoning/inf/inf.n3>].

skolem:lemma12 a r:Inference;
    r:gives {
