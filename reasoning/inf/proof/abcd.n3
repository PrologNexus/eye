    r:rule skolem:lemma8.

skolem:lemma7 a r:Inference;
    r:gives {
        {
            {
                :i a :D.
            } => ().
            :i a :A.
        } => {
            :i a :B.
        }.
    };
    r:evidence (
        skolem:lemma9
        [ a r:Fact; r:gives {({
            :i a :B.
        }) list:removeDuplicates ({
            :i a :B.
        })}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo {
        {
            :i a :D.
        } => ().
