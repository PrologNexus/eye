@prefix log: <http://www.w3.org/2000/10/swap/log#>.

# rewrite
{
    false log:or ?A.
} => {
    true => ?A.
}.

{
    ?A log:or false.
} => {
    true => ?A.
}.

# reorder
{
    ?A log:or {
        ?B log:or ?C.
    }.
} => {
    true => {
        ?C log:or ?B.
    }.
}.

{
    ?A log:or {
        ?B log:or ?C.
    }.
} => {
    true => {
        ?B log:or {
            ?A log:or ?C.
        }.
    }.
}.

# resolve
{
    ?A => {
        ?B log:or ?C.
    }.
    ?B => ?D.
    ?C => ?E.
} => {
    ?A => {
        ?D log:or ?E.
    }.
}.

{
    ?A => ?B.
    ?B => false.
} => {
    true => {
        ?A => false.
    }.
}.

{
    ?A => {
        ?B log:or ?C.
    }.
    ?B => false.
} => {
    ?A => ?C.
}.

{
    ?A => {
        ?B log:or ?C.
    }.
    ?C => false.
} => {
    ?A => ?B.
}.

# factor
{
    ?A => {
        ?B log:or ?B.
    }.
} => {
    ?A => ?B.
}.
