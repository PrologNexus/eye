@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        :A a :Wolf.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :A a :Wolf.
    };
    r:evidence (
        skolem:lemma2
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#A"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ n3:uri "http://example.org/#Wolf"]];
    r:rule skolem:lemma3.

skolem:lemma2 a r:Inference;
    r:gives {
        :A a :Wolf.
    };
    r:evidence (
        [ a r:Fact; r:gives true]
    );
    r:rule skolem:lemma4.

skolem:lemma3 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1. {
            var:x_0 a var:x_1.
        } => {
            var:x_0 a var:x_1.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/inf/test2.n3>].

skolem:lemma4 a r:Inference;
    r:gives {
        true => {
            :A a :Wolf.
        }.
    };
    r:evidence (
        skolem:lemma5
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo {
        :A a :Wolf.
    }];
    r:rule skolem:lemma6.

skolem:lemma5 a r:Inference;
    r:gives {
        {
            :A a :Wolf.
        } log:or false.
    };
    r:evidence (
        [ a r:Fact; r:gives true]
    );
    r:rule skolem:lemma7.

skolem:lemma6 a r:Extraction;
    r:gives {
        @forAll var:x_0. {
            var:x_0 log:or false.
        } => {
            true => var:x_0.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/inf/inf.n3>].

skolem:lemma7 a r:Extraction;
    r:gives {
        true => {
            {
                :A a :Wolf.
            } log:or false.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/inf/test2.n3>].

