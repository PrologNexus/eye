@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <https://eyereasoner.github.io/ns#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        (1.0 1.0e-15) :findRoot 1.4142135623730951 .
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        (1.0 1.0e-15) :findRoot 1.4142135623730951 .
    };
    r:evidence (
        [ a r:Fact; r:gives {("consult" "./newton-raphson.pl") log:core true}]
        [ a r:Fact; r:gives {(1.0 1.0e-15) :findRoot 1.4142135623730951}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo 1.4142135623730951];
    r:rule skolem:lemma2.

skolem:lemma2 a r:Extraction;
    r:gives {
        @forAll var:x_0. {
            ("consult" "./newton-raphson.pl") log:core true.
            (1.0 1.0e-15) :findRoot var:x_0.
        } => {
            (1.0 1.0e-15) :findRoot var:x_0.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/newton-raphson/newton-raphson-query.n3>].

