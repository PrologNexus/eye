# Disjunction example 2

@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

(_S) log:onNegativeSurface {
    _S a :C0.
    () log:onNegativeSurface {
        _S a :C1.
    }.
    () log:onNegativeSurface {
        _S a :C2.
    }.
}.

(_S) log:onNegativeSurface {
    _S a :C1.
    () log:onNegativeSurface {
        _S a :C11.
    }.
    () log:onNegativeSurface {
        _S a :C12.
    }.
}.

(_S) log:onNegativeSurface {
    _S a :C2.
    () log:onNegativeSurface {
        _S a :C21.
    }.
    () log:onNegativeSurface {
        _S a :C22.
    }.
}.

(_S) log:onNegativeSurface {
    _S a :C11.
    () log:onNegativeSurface {
        _S a :C111.
    }.
    () log:onNegativeSurface {
        _S a :C112.
    }.
}.

(_S) log:onNegativeSurface {
    _S a :C12.
    () log:onNegativeSurface {
        _S a :C121.
    }.
    () log:onNegativeSurface {
        _S a :C122.
    }.
}.

(_S) log:onNegativeSurface {
    _S a :C21.
    () log:onNegativeSurface {
        _S a :C211.
    }.
    () log:onNegativeSurface {
        _S a :C212.
    }.
}.

(_S) log:onNegativeSurface {
    _S a :C22.
    () log:onNegativeSurface {
        _S a :C221.
    }.
    () log:onNegativeSurface {
        _S a :C222.
    }.
}.

# data
:abc a :C0.

() log:onNegativeSurface {
    :abc a :C1.
}.

() log:onNegativeSurface {
    :abc a :C21.
}.

() log:onNegativeSurface {
    :abc a :C221.
}.

# query
(_A _B) log:onQuerySurface {
    _A a _B.
}.
