# Disjunction example 2

@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

(?S) log:onNegativeSurface {
    ?S a :C0.
    () log:onNegativeSurface {
        ?S a :C1.
    }.
    () log:onNegativeSurface {
        ?S a :C2.
    }.
}.

(?S) log:onNegativeSurface {
    ?S a :C1.
    () log:onNegativeSurface {
        ?S a :C11.
    }.
    () log:onNegativeSurface {
        ?S a :C12.
    }.
}.

(?S) log:onNegativeSurface {
    ?S a :C2.
    () log:onNegativeSurface {
        ?S a :C21.
    }.
    () log:onNegativeSurface {
        ?S a :C22.
    }.
}.

(?S) log:onNegativeSurface {
    ?S a :C11.
    () log:onNegativeSurface {
        ?S a :C111.
    }.
    () log:onNegativeSurface {
        ?S a :C112.
    }.
}.

(?S) log:onNegativeSurface {
    ?S a :C12.
    () log:onNegativeSurface {
        ?S a :C121.
    }.
    () log:onNegativeSurface {
        ?S a :C122.
    }.
}.

(?S) log:onNegativeSurface {
    ?S a :C21.
    () log:onNegativeSurface {
        ?S a :C211.
    }.
    () log:onNegativeSurface {
        ?S a :C212.
    }.
}.

(?S) log:onNegativeSurface {
    ?S a :C22.
    () log:onNegativeSurface {
        ?S a :C221.
    }.
    () log:onNegativeSurface {
        ?S a :C222.
    }.
}.

# data
:abc a :C0.

() log:onNegativeSurface {
    :abc a :C1.
}.

() log:onNegativeSurface {
    :abc a :C21.
}.

() log:onNegativeSurface {
    :abc a :C221.
}.

# query
(?A ?B) log:onQuerySurface {
    ?A a ?B.
}.
