@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

# ~(A . ~(B . ~A)) = ~A whatever B is 
() log:onNegativeSurface {
    :A a :AA .

    () log:onNegativeSurface {
        # B
        #:B a :BB .

        () log:onNegativeSurface {
            :A a :AA .
        } .
    } .
} .


(skolem:S skolem:O) log:onQuerySurface {
    () log:onNegativeSurface {
        skolem:S a skolem:O .
    } .
} .
