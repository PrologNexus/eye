@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

:i a :A.

() log:onNegativeSurface {
    :i a :C.
}.

() log:onNegativeSurface {
    () log:onNegativeSurface {
        () log:onNegativeSurface {
            :i a :D.
        }.
    }.
}.

(_S) log:onNegativeSurface {
    _S a :A.
    () log:onNegativeSurface {
        _S a :B.
    }.
    () log:onNegativeSurface {
        _S a :C.
    }.
    () log:onNegativeSurface {
        _S a :D.
    }.
}.

# query
(_S _C) log:onQuerySurface {
    _S a _C.
}.
