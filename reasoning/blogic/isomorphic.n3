@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

(skolem:S1 skolem:S2) log:onNegativeSurface {

    skolem:S1 log:equalTo {
        (skolem:V) log:onNegativeSurface {
            skolem:V a :C .
        } .
    } .

    skolem:S2 log:equalTo {
        (skolem:E) log:onNegativeSurface {
            skolem:E a :C .
        } .
    } .

    skolem:S1 log:isomorphic skolem:S2 .

    () log:onNegativeSurface {
        :test :is true .
    } .
} .

() log:onQuerySurface {
    :test :is true .
} .
