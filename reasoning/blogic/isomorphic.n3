@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

(?S1 ?S2) log:onNegativeSurface {

    ?S1 log:equalTo {
        (?V) log:onNegativeSurface {
            ?V a :C .
        } .
    } .

    ?S2 log:equalTo {
        (?E) log:onNegativeSurface {
            ?E a :C .
        } .
    } .

    ?S1 log:isomorphic ?S2 .

    () log:onNegativeSurface {
        :test :is true .
    } .
} .

() log:onQuerySurface {
    :test :is true .
} .
