@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

() log:onNeutralSurface {
    ?F a :C .
} .

() log:onNegativeSurface {
    ?E a :C .
} .

(?a ?b ?S1 ?S2) log:onNegativeSurface {

    ?a log:onNegativeSurface ?S1 .
    ?b log:onNeutralSurface ?S2 .

    ?S1 log:notIncludes ?S2 .

    () log:onNegativeSurface {
        :test :is true .
    } .
} .

() log:onQuerySurface {
    :test :is true .
} .
