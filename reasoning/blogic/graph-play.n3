@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

() log:onNeutralSurface {
    _F a :C .
} .

() log:onNegativeSurface {
    _E a :C .
} .

(_a _b _S1 _S2) log:onNegativeSurface {

    _a log:onNegativeSurface _S1 .
    _b log:onNeutralSurface _S2 .

    _S1 log:notIncludes _S2 .

    () log:onNegativeSurface {
        :test :is true .
    } .
} .

() log:onQuerySurface {
    :test :is true .
} .
