@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/d7882a29-44b8-4cbc-bc70-3bd11fba1fdb#>.

() log:onNeutralSurface {
    skolem:F a :C .
} .

() log:onNegativeSurface {
    skolem:E a :C .
} .

(skolem:a skolem:b skolem:S1 skolem:S2) log:onNegativeSurface {

    skolem:a log:onNegativeSurface skolem:S1 .
    skolem:b log:onNeutralSurface skolem:S2 .

    skolem:S1 log:notIncludes skolem:S2 .

    () log:onNegativeSurface {
        :test :is true .
    } .
} .

() log:onQuerySurface {
    :test :is true .
} .
