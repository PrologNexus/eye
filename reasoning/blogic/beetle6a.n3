@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.

# beetle is not blue
() log:onNegativeSurface {
    :beetle :is :blue.
}.

# all cars are green or blue
(_A) log:onNegativeSurface {
    _A a :Car.
    () log:onNegativeSurface {
        _A :is :green.
    }.
    () log:onNegativeSurface {
        _A :is :blue.
    }.
}.

# green things are nice or pretty
(_A) log:onNegativeSurface {
    _A :is :green.
    () log:onNegativeSurface {
        _A :is :nice.
    }.
    () log:onNegativeSurface {
        _A :is :pretty.
    }.
}.
# pretty things are beautiful
(_A) log:onNegativeSurface {
    _A :is :pretty.
    () log:onNegativeSurface {
        _A :is :beautiful.
    }.
}.

# cars are not beautiful
(_A) log:onNegativeSurface {
    _A a :Car.
    () log:onPositiveSurface {
        _A :is :beautiful.
    }.
}.

# blue things are beautiful
(_A) log:onNegativeSurface {
    _A :is :blue.
    () log:onNegativeSurface {
        _A :is :beautiful.
    }.
}.

# query
(_S) log:onQuerySurface {
    _S :is :nice.
}.
