# -----------------------
# Basic Monadic Benchmark
# -----------------------
#
# Tested with input of 10^n triples generated with http://josd.github.io/bmb/graphgen.n3

@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://eulersharp.sourceforge.net/2007/07test/graph#>.
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/d7882a29-44b8-4cbc-bc70-3bd11fba1fdb#>.

() log:onNegativeSurface {
    <> log:imports <../bmt/1tt1.ttl>.
    <> log:imports <../bmt/1tt2.ttl>.
    <> log:imports <../bmt/1tt3.ttl>.
    <> log:imports <../bmt/1tt4.ttl>.
    <> log:imports <../bmt/1tt5.ttl>.
    <> log:imports <../bmt/1tt6.ttl>.
    <> log:imports <../bmt/1tt7.ttl>.
    <> log:imports <../bmt/1tt8.ttl>.
    <> log:imports <../bmt/1tt9.ttl>.
    <> log:imports <../bmt/1tt10.ttl>.
    () log:onNegativeSurface true.
}.

(skolem:R skolem:D0 skolem:D1 skolem:D2 skolem:D3 skolem:D4 skolem:D5 skolem:D6 skolem:D7 skolem:D8 skolem:D9) log:onNegativeSurface {
    () log:onNegativeSurface <<skolem:R :cycle (skolem:D0 skolem:D1 skolem:D2 skolem:D3 skolem:D4 skolem:D5 skolem:D6 skolem:D7 skolem:D8 skolem:D9)>>.
    skolem:D0 skolem:R skolem:D1.
    skolem:D1 skolem:R skolem:D2.
    skolem:D2 skolem:R skolem:D3.
    skolem:D3 skolem:R skolem:D4.
    skolem:D4 skolem:R skolem:D5.
    skolem:D5 skolem:R skolem:D6.
    skolem:D6 skolem:R skolem:D7.
    skolem:D7 skolem:R skolem:D8.
    skolem:D8 skolem:R skolem:D9.
    skolem:D9 skolem:R skolem:D0.
}.

# query
(skolem:R skolem:C) log:onQuerySurface {
    skolem:R :cycle skolem:C.
}.
