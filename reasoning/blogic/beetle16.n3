@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.

# beetle is not beautiful
() log:onNegativeSurface {
    :beetle :is :beautiful.
}.

# if something is a car and not beautiful it is not new
(skolem:A) log:onNegativeSurface {
    skolem:A a :Car.
    () log:onNegativeSurface {
        skolem:A :is :beautiful.
    }.
    () log:onNegativeSurface {
        () log:onNegativeSurface {
            skolem:A :is :new.
        }.
    }.
}.

# if something is not new  it is old
(skolem:A) log:onNegativeSurface {
    () log:onNegativeSurface {
        skolem:A :is :new.
    }.
    () log:onNegativeSurface {
        skolem:A :is :old.
    }.
}.

# query
(skolem:S skolem:O) log:onQuerySurface {
    skolem:S :is skolem:O.
}.
