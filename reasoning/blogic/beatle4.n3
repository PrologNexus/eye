@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

:beatle a :Car.

# all cars are maybe green and maybe blue
(_:A) log:onNegativeSurface {
    _:A a :Car.
    () log:onNegativeSurface {
        () log:onNeutralSurface {
            _:A :is :green.
        }.
        () log:onNeutralSurface {
            _:A :is :blue.
        }.
    }.
}.

# maybe green and maybe blue things are beautiful
(_:A) log:onNegativeSurface {
    () log:onNeutralSurface {
        _:A :is :blue.
    }.
    () log:onNeutralSurface {
        _:A :is :green.
    }.
    () log:onNegativeSurface {
        _:A :is :beautiful.
    }.
}.

# ask if beatle is car and beautiful
() log:onQuerySurface {
    :beatle a :Car.
    :beatle :is :beautiful.
}.
