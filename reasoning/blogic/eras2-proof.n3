@prefix : <urn:example:>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix var: <http://eyereasoner.github.io/var#>.
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/3917894951695475063#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        () log:onNegativeSurface {
            <urn:example:A> a <urn:example:AA>.
        }.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        () log:onNegativeSurface {
            <urn:example:A> a <urn:example:AA>.
        }.
    };
    r:evidence (
        [ a r:Fact; r:gives {() log:onNegativeSurface {
            <urn:example:A> a <urn:example:AA>.
        }}]
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ n3:uri "urn:example:A"]];
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_1"]; r:boundTo [ n3:uri "urn:example:AA"]];
    r:rule [ a r:DerivedQuery; r:gives {
        {
            () log:onNegativeSurface {
                var:x_0 a var:x_1.
            }.
        } => {
            () log:onNegativeSurface {
                var:x_0 a var:x_1.
            }.
        }.
    }].

