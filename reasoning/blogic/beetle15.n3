@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.

# cars are green or blue
(_A) log:onNegativeSurface {
    _A a :Car.
    () log:onNegativeSurface {
        () log:onNegativeSurface {
            () log:onNegativeSurface {
                _A :is :green.
            }.
            () log:onNegativeSurface {
                _A :is :blue.
            }.
        }.
    }.
}.

# if something is green or blue it is a beautiful (but backwards)
(_A) log:onNegativeSurface {
    () log:onNegativeSurface {
        () log:onNegativeSurface {
            _A :is :green.
        }.
        () log:onNegativeSurface {
            _A :is :blue.
        }.
    }.
    () log:onNegativeSurface <<_A :is :beautiful>>.
}.

# query
(_S _O) log:onQuerySurface {
    _S :is _O.
}.
