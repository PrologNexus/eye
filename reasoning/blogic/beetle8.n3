@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.

# beetle is not blue
() log:onNegativeSurface {
    :beetle :is :blue.
}.

# beetle is not nice
() log:onNegativeSurface {
    :beetle :is :nice.
}.

# all cars are green or blue
(skolem:A) log:onNegativeSurface {
    skolem:A a :Car.
    () log:onNegativeSurface {
        skolem:A :is :green.
    }.
    () log:onNegativeSurface {
        skolem:A :is :blue.
    }.
}.

# green things are nice or beautiful
(skolem:A) log:onNegativeSurface {
    skolem:A :is :green.
    () log:onNegativeSurface {
        skolem:A :is :nice.
    }.
    () log:onNegativeSurface {
        skolem:A :is :beautiful.
    }.
}.

# blue things are beautiful
(skolem:A) log:onNegativeSurface {
    skolem:A :is :blue.
    () log:onNegativeSurface {
        skolem:A :is :beautiful.
    }.
}.

# query
(skolem:S skolem:O) log:onQuerySurface {
    skolem:S :is skolem:O.
}.
