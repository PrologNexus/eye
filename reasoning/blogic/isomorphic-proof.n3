@prefix : <urn:example:>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix var: <http://eyereasoner.github.io/var#>.
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        <urn:example:test> <urn:example:is> true.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        <urn:example:test> <urn:example:is> true.
    };
    r:evidence (
        skolem:lemma2
    );
    r:rule [ a r:DerivedQuery; r:gives {
        {
            <urn:example:test> <urn:example:is> true.
        } => {
            <urn:example:test> <urn:example:is> true.
        }.
    }].

skolem:lemma2 a r:Inference;
    r:gives {
        <urn:example:test> <urn:example:is> true.
    };
    r:evidence (
        [ a r:Fact; r:gives {{
            (skolem:e_V_1_2) log:onNegativeSurface {
                skolem:e_V_1_2 a <urn:example:C>.
            }.
        } log:equalTo {
            (skolem:e_V_1_2) log:onNegativeSurface {
                skolem:e_V_1_2 a <urn:example:C>.
            }.
        }}]
        [ a r:Fact; r:gives {{
            (skolem:e_E_1_2) log:onNegativeSurface {
                skolem:e_E_1_2 a <urn:example:C>.
            }.
        } log:equalTo {
            (skolem:e_E_1_2) log:onNegativeSurface {
                skolem:e_E_1_2 a <urn:example:C>.
            }.
        }}]
        [ a r:Fact; r:gives {{
            (skolem:e_V_1_2) log:onNegativeSurface {
                skolem:e_V_1_2 a <urn:example:C>.
            }.
        } log:isomorphic {
            (skolem:e_E_1_2) log:onNegativeSurface {
                skolem:e_E_1_2 a <urn:example:C>.
            }.
        }}]
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo {
        (skolem:e_V_1_2) log:onNegativeSurface {
            skolem:e_V_1_2 a <urn:example:C>.
        }.
    }];
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_1"]; r:boundTo {
        (skolem:e_E_1_2) log:onNegativeSurface {
            skolem:e_E_1_2 a <urn:example:C>.
        }.
    }];
    r:rule [ a r:DerivedRule; r:gives {
        {
            var:x_0 log:equalTo {
                (skolem:e_V_1_2) log:onNegativeSurface {
                    skolem:e_V_1_2 a <urn:example:C>.
                }.
            }.
            var:x_1 log:equalTo {
                (skolem:e_E_1_2) log:onNegativeSurface {
                    skolem:e_E_1_2 a <urn:example:C>.
                }.
            }.
            var:x_0 log:isomorphic var:x_1.
        } => {
            <urn:example:test> <urn:example:is> true.
        }.
    }].

