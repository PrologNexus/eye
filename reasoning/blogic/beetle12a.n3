@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.

# all cars are green or blue
(?A) log:onNegativeSurface {
    ?A a :Car.
    () log:onNegativeSurface {
        ?A :is :green.
    }.
    () log:onNegativeSurface {
        ?A :is :blue.
    }.
}.

# green things are nice or pretty
(?A) log:onNegativeSurface {
    ?A :is :green.
    () log:onNegativeSurface {
        ?A :is :nice.
    }.
    () log:onNegativeSurface {
        ?A :is :pretty.
    }.
}.

# nice or pretty or blue things are beautiful
(?A) log:onNegativeSurface {
    () log:onNegativeSurface {
        () log:onNegativeSurface {
            ?A :is :nice.
        }.
        () log:onNegativeSurface {
            ?A :is :pretty.
        }.
        () log:onNegativeSurface {
            ?A :is :blue.
        }.
    }.
    () log:onNegativeSurface {
        ?A :is :beautiful.
    }.
}.

# query
(?S ?O) log:onQuerySurface {
    ?S :is ?O.
}.
