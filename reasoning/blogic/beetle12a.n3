@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/d7882a29-44b8-4cbc-bc70-3bd11fba1fdb#>.

# beetle is a car
:beetle a :Car.

# all cars are green or blue
(skolem:A) log:onNegativeSurface {
    skolem:A a :Car.
    () log:onNegativeSurface {
        skolem:A :is :green.
    }.
    () log:onNegativeSurface {
        skolem:A :is :blue.
    }.
}.

# green things are nice or pretty
(skolem:A) log:onNegativeSurface {
    skolem:A :is :green.
    () log:onNegativeSurface {
        skolem:A :is :nice.
    }.
    () log:onNegativeSurface {
        skolem:A :is :pretty.
    }.
}.

# nice or pretty or blue things are beautiful
(skolem:A) log:onNegativeSurface {
    () log:onNegativeSurface {
        () log:onNegativeSurface {
            skolem:A :is :nice.
        }.
        () log:onNegativeSurface {
            skolem:A :is :pretty.
        }.
        () log:onNegativeSurface {
            skolem:A :is :blue.
        }.
    }.
    () log:onNegativeSurface {
        skolem:A :is :beautiful.
    }.
}.

# query
(skolem:S skolem:O) log:onQuerySurface {
    skolem:S :is skolem:O.
}.
