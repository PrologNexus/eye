@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

(_E) log:onNegativeSurface {
    _E a :C .
} .

(_a _S1) log:onNegativeSurface {

    _a log:onNegativeSurface _S1.

    () log:onNegativeSurface {
        () log:onNeutralSurface _S1.
    } .
} .

(_S1 _x _y) log:onQuerySurface {
    () log:onNeutralSurface _S1.
    _x log:onNegativeSurface _y.
} .
