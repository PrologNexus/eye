@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

(?E) log:onNegativeSurface {
    ?E a :C .
} .

(?a ?S1) log:onNegativeSurface {

    ?a log:onNegativeSurface ?S1.

    () log:onNegativeSurface {
        () log:onNeutralSurface ?S1.
    } .
} .

(?S1 ?x ?y) log:onQuerySurface {
    () log:onNeutralSurface ?S1.
    ?x log:onNegativeSurface ?y.
} .
