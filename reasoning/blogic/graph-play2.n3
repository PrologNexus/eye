@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/d7882a29-44b8-4cbc-bc70-3bd11fba1fdb#>.

(skolem:E) log:onNegativeSurface {
    skolem:E a :C .
} .

(skolem:a skolem:S1) log:onNegativeSurface {

    skolem:a log:onNegativeSurface skolem:S1.

    () log:onNegativeSurface {
        () log:onNeutralSurface skolem:S1.
    } .
} .

(skolem:S1 skolem:x skolem:y) log:onQuerySurface {
    () log:onNeutralSurface skolem:S1.
    skolem:x log:onNegativeSurface skolem:y.
} .
