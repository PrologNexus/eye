@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.

# all cars are green or blue
(?A) log:onNegativeSurface {
    ?A a :Car.
    () log:onNegativeSurface {
        ?A :is :green.
    }.
    () log:onNegativeSurface {
        ?A :is :blue.
    }.
}.

# green things are nice or pretty
(?A) log:onNegativeSurface {
    ?A :is :green.
    () log:onNegativeSurface {
        ?A :is :nice.
    }.
    () log:onNegativeSurface {
        ?A :is :pretty.
    }.
}.

# pretty things are pretty 1 or pretty 2
(?A) log:onNegativeSurface {
    ?A :is :pretty.
    () log:onNegativeSurface {
        ?A :is :pretty1.
    }.
    () log:onNegativeSurface {
        ?A :is :pretty2.
    }.
}.

# nice things are nice 1 or nice 2
(?A) log:onNegativeSurface {
    ?A :is :nice.
    () log:onNegativeSurface {
        ?A :is :nice1.
    }.
    () log:onNegativeSurface {
        ?A :is :nice2.
    }.
}.

# pretty 1 things are pretty 11 or pretty 12
(?A) log:onNegativeSurface {
    ?A :is :pretty1.
    () log:onNegativeSurface {
        ?A :is :pretty11.
    }.
    () log:onNegativeSurface {
        ?A :is :pretty12.
    }.
}.

# pretty 2 things are pretty 21 or pretty 22
(?A) log:onNegativeSurface {
    ?A :is :pretty2.
    () log:onNegativeSurface {
        ?A :is :pretty21.
    }.
    () log:onNegativeSurface {
        ?A :is :pretty22.
    }.
}.

# nice 1 things are nice 11 or nice 12
(?A) log:onNegativeSurface {
    ?A :is :nice1.
    () log:onNegativeSurface {
        ?A :is :nice11.
    }.
    () log:onNegativeSurface {
        ?A :is :nice12.
    }.
}.

# nice 2 things are nice 21 or nice 22
(?A) log:onNegativeSurface {
    ?A :is :nice2.
    () log:onNegativeSurface {
        ?A :is :nice21.
    }.
    () log:onNegativeSurface {
        ?A :is :nice22.
    }.
}.

# pretty or nice or blue things are beautiful
(?A) log:onNegativeSurface {
    () log:onNegativeSurface {
        () log:onNegativeSurface {
            ?A :is :pretty11.
        }.
        () log:onNegativeSurface {
            ?A :is :pretty12.
        }.
        () log:onNegativeSurface {
            ?A :is :pretty21.
        }.
        () log:onNegativeSurface {
            ?A :is :pretty22.
        }.
        () log:onNegativeSurface {
            ?A :is :nice11.
        }.
        () log:onNegativeSurface {
            ?A :is :nice12.
        }.
        () log:onNegativeSurface {
            ?A :is :nice21.
        }.
        () log:onNegativeSurface {
            ?A :is :nice22.
        }.
        () log:onNegativeSurface {
            ?A :is :blue.
        }.
    }.
    () log:onNegativeSurface {
        ?A :is :beautiful.
    }.
}.

# query
(?S ?O) log:onQuerySurface {
    ?S :is ?O.
}.
