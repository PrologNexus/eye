@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.

# bob is a person
:bob a :Person.

# grannysmith apple is green
:grannysmith a :Apple ;
    :is :green .

# mars is the red planet
:mars a :Planet.

# green things are beautiful
(skolem:A) log:onNegativeSurface {
    skolem:A :is :green.
    () log:onNegativeSurface {
        skolem:A :is :beautiful.
    }.
}.

# things which are not green are beautiful
(skolem:A) log:onNegativeSurface {
    () log:onNegativeSurface {
        skolem:A :is :green
    }.
    () log:onNegativeSurface {
        skolem:A :is :beautiful.
    }.
}.

# triples are composed of rdfs resources
(skolem:S skolem:P skolem:O) log:onNegativeSurface {
    skolem:S skolem:P skolem:O.
    () log:onNegativeSurface {
        skolem:S a rdfs:Resource.
        skolem:P a rdfs:Resource.
        skolem:O a rdfs:Resource.
    }.
}.

# query
(skolem:S skolem:O) log:onQuerySurface {
    skolem:S :is skolem:O.
}.
