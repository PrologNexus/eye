@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.

# all cars are green or blue
(skolem:A) log:onNegativeSurface {
    skolem:A a :Car.
    () log:onNegativeSurface {
        skolem:A :is :green.
    }.
    () log:onNegativeSurface {
        skolem:A :is :blue.
    }.
}.

# green things are nice or pretty
(skolem:A) log:onNegativeSurface {
    skolem:A :is :green.
    () log:onNegativeSurface {
        skolem:A :is :nice.
    }.
    () log:onNegativeSurface {
        skolem:A :is :pretty.
    }.
}.

# pretty things are beautiful
(skolem:A) log:onNegativeSurface {
    skolem:A :is :pretty.
    () log:onNegativeSurface {
        skolem:A :is :beautiful.
    }.
}.

# cars are not pretty or not nice
(skolem:A) log:onNegativeSurface {
    skolem:A a :Car.
    skolem:A :is :pretty.
    skolem:A :is :nice.
}.

# blue things are beautiful
(skolem:A) log:onNegativeSurface {
    skolem:A :is :blue.
    () log:onNegativeSurface {
        skolem:A :is :beautiful.
    }.
}.

# query
(skolem:S) log:onQuerySurface {
    :beetle a skolem:S.
}.

(skolem:S) log:onQuerySurface {
    :beetle :is skolem:S.
}.
