@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.

# all cars are green or blue
(_A) log:onNegativeSurface {
    _A a :Car.
    () log:onNegativeSurface {
        _A :is :green.
    }.
    () log:onNegativeSurface {
        _A :is :blue.
    }.
}.

# green things are nice or pretty
(_A) log:onNegativeSurface {
    _A :is :green.
    () log:onNegativeSurface {
        _A :is :nice.
    }.
    () log:onNegativeSurface {
        _A :is :pretty.
    }.
}.

# pretty things are beautiful
(_A) log:onNegativeSurface {
    _A :is :pretty.
    () log:onNegativeSurface {
        _A :is :beautiful.
    }.
}.

# cars are not pretty or not nice
(_A) log:onNegativeSurface {
    _A a :Car.
    _A :is :pretty.
    _A :is :nice.
}.

# blue things are beautiful
(_A) log:onNegativeSurface {
    _A :is :blue.
    () log:onNegativeSurface {
        _A :is :beautiful.
    }.
}.

# query
(_S) log:onQuerySurface {
    :beetle a _S.
}.

(_S) log:onQuerySurface {
    :beetle :is _S.
}.
