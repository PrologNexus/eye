@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix var: <http://eyereasoner.github.io/var#>.
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:component skolem:lemma2;
    r:gives {
        :beetle :is :green.
        :beetle :is :beautiful.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :beetle :is :green.
    };
    r:evidence (
        skolem:lemma3
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ n3:uri "http://example.org/ns#beetle"]];
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_1"]; r:boundTo [ n3:uri "http://example.org/ns#green"]];
    r:rule [ a r:DerivedQuery; r:gives {
        {
            var:x_0 :is var:x_1.
        } => {
            var:x_0 :is var:x_1.
        }.
    }].

skolem:lemma2 a r:Inference;
    r:gives {
        :beetle :is :beautiful.
    };
    r:evidence (
        skolem:lemma4
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ n3:uri "http://example.org/ns#beetle"]];
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_1"]; r:boundTo [ n3:uri "http://example.org/ns#beautiful"]];
    r:rule [ a r:DerivedQuery; r:gives {
        {
            var:x_0 :is var:x_1.
        } => {
            var:x_0 :is var:x_1.
        }.
    }].

skolem:lemma3 a r:Inference;
    r:gives {
        :beetle :is :green.
    };
    r:evidence (
        skolem:lemma5
        skolem:lemma6
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ n3:uri "http://example.org/ns#beetle"]];
    r:rule [ a r:DerivedRule; r:gives {
        {
            var:x_0 a :Car.
            () log:onNegativeSurface {
                var:x_0 :is :blue.
            }.
        } => {
            var:x_0 :is :green.
        }.
    }].

skolem:lemma4 a r:Inference;
    r:gives {
        :beetle :is :beautiful.
    };
    r:evidence (
        skolem:lemma3
        skolem:lemma7
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ n3:uri "http://example.org/ns#beetle"]];
    r:rule [ a r:DerivedRule; r:gives {
        {
            var:x_0 :is :green.
            () log:onNegativeSurface {
                var:x_0 :is :nice.
            }.
        } => {
            var:x_0 :is :beautiful.
        }.
    }].

skolem:lemma5 a r:Extraction;
    r:gives {
        :beetle a :Car.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/blogic/beetle8.n3s>].

skolem:lemma6 a r:Extraction;
    r:gives {
        () log:onNegativeSurface {
            :beetle :is :blue.
        }.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/blogic/beetle8.n3s>].

skolem:lemma7 a r:Extraction;
    r:gives {
        () log:onNegativeSurface {
            :beetle :is :nice.
        }.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/blogic/beetle8.n3s>].

