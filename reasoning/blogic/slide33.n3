@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/d7882a29-44b8-4cbc-bc70-3bd11fba1fdb#>.

# slide 33 example from https://www.slideshare.net/PatHayes/blogic-iswc-2009-invited-talk

# owl restriction
:aaa owl:onProperty :bbb.
:aaa owl:allValuesFrom :ccc.

# the following 2 triples should entail :yyy a :ccc.
#:xxx a :aaa.
#:xxx :bbb :yyy.

# the following codex should entail :xxx a :aaa.
(skolem:y) log:onNegativeSurface {
    :xxx :bbb skolem:y.
    () log:onNegativeSurface {
        skolem:y a :ccc.
    }.
}.

# owl:allValuseFrom description logic
(skolem:a skolem:b skolem:c) log:onNegativeSurface {
    skolem:a owl:onProperty skolem:b.
    skolem:a owl:allValuesFrom skolem:c.
    () log:onNegativeSurface {
        (skolem:x skolem:y) log:onNegativeSurface {
            skolem:x a skolem:a.
            skolem:x skolem:b skolem:y.
            () log:onNegativeSurface {
                skolem:y a skolem:c.
            }.
        }.
        (skolem:x) log:onNegativeSurface {
            (skolem:y) log:onNegativeSurface {
                skolem:x skolem:b skolem:y.
                () log:onNegativeSurface {
                    skolem:y a skolem:c.
                }.
            }.
            () log:onNegativeSurface {
                skolem:x a skolem:a.
            }.
        }.
    }.
}.

# query
(skolem:S skolem:C) log:onQuerySurface {
    skolem:S a skolem:C.
}.
