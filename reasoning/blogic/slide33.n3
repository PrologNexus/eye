@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

:aaa owl:onProperty :bbb.
:aaa owl:allValuesFrom :ccc.

() log:negativeSurface {
    :xxx :bbb :yyy.
    () log:negativeSurface {
        :yyy a :ccc.
    }.
}.

(_:a _:b _:c) log:negativeSurface {
    _:a owl:onProperty _:b.
    _:a owl:allValuesFrom _:c.
    () log:negativeSurface {
        (_:x _:y) log:negativeSurface {
            _:x a _:a.
            _:x _:b _:y.
            () log:negativeSurface {
                _:y a _:c.
            }.
        }.
        (_:x _:y) log:negativeSurface {
            () log:negativeSurface {
                _:x _:b _:y.
                () log:negativeSurface {
                    _:y a _:c.
                }.
            }.
            () log:negativeSurface {
                _:x a _:a.
            }.
        }.
    }.
}.

# query
(_:S _:C) log:querySurface {
    _:S a _:C.
}.
