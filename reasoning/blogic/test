#!/bin/bash
for f in *.n3
do
    echo + eye --quiet --skolem-genid 8b98b360-9a70-4845-b52c-c675af60ad01 --nope $f --output $f.out
    eye --quiet --skolem-genid 8b98b360-9a70-4845-b52c-c675af60ad01 --nope $f --output $f.out
done

# extra check when using --pass-only-new
echo + eye --quiet --skolem-genid 8b98b360-9a70-4845-b52c-c675af60ad01 --nope alice.n3 --pass-only-new --output alice.n3.out
eye --quiet --skolem-genid 8b98b360-9a70-4845-b52c-c675af60ad01 --nope alice.n3 --pass-only-new --output alice.n3.out
