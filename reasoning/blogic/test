#!/bin/bash
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/abc.n3 > abc-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/abcd.n3 > abcd-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/complex.n3 > complex-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/fibonacci.n3 > fibonacci-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/rain.n3 > rain-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/slide32.n3 > slide32-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/slide33.n3 > slide33-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/socrates.n3 > socrates-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/socrates-star.n3 > socrates-star-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --turtle http://josd.github.io/eye/reasoning/bmt/1tt1.n3 --turtle http://josd.github.io/eye/reasoning/bmt/1tt2.n3 --turtle http://josd.github.io/eye/reasoning/bmt/1tt3.n3 --turtle http://josd.github.io/eye/reasoning/bmt/1tt4.n3 --turtle http://josd.github.io/eye/reasoning/bmt/1tt5.n3 --turtle http://josd.github.io/eye/reasoning/bmt/1tt6.n3 --turtle http://josd.github.io/eye/reasoning/bmt/1tt7.n3 --turtle http://josd.github.io/eye/reasoning/bmt/1tt8.n3 --turtle http://josd.github.io/eye/reasoning/bmt/1tt9.n3 --turtle http://josd.github.io/eye/reasoning/bmt/1tt10.n3 --blogic http://josd.github.io/eye/reasoning/blogic/bmt.n3 > bmt-answer.n3
