(_A _B _C _D _E _F _R _T _Z1 _Z2 _Z3 _Z4 _Z5 _Z6 _Z7 _Z8 _Z9 _Z10) log:onNegativeSurface {
    () log:onNegativeSurface <<((_A _B) (_C _D)) complex:exponentiation (_E _F)>>.
    (_A _B) complex:polar (_R _T).
    (_R _C) math:exponentiation _Z1.
    _D math:negation _Z2.
    (_Z2 _T) math:product _Z3.
    (2.718281828459045 _Z3) math:exponentiation _Z4.
    (2.718281828459045 _Z5) math:exponentiation _R.
    (_D _Z5) math:product _Z6.
    (_C _T) math:product _Z7.
    (_Z6 _Z7) math:sum _Z8.
    _Z8 math:cos _Z9.
    (_Z1 _Z4 _Z9) math:product _E.
    _Z8 math:sin _Z10.
    (_Z1 _Z4 _Z10) math:product _F.
}.

(_A _B _C _D _E _F _Z1 _Z2 _Z3 _Z4 _Z5 _Z6 _Z7 _Z8 _Z9 _Z10 _Z11 _Z12 _Z13 _Z14 _Z15) log:onNegativeSurface {
