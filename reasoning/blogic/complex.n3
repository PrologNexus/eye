    (_Z6 2) math:exponentiation _Z7.
    (_Z7 _Z3) math:sum _Z8.
    (_Z8 0.5) math:exponentiation _Z9.
    (_Z5 _Z9) math:difference _Z10.
    (_Z10 2) math:quotient _E.
    (_Z5 _Z9) math:sum _Z11.
    (_Z11 2) math:quotient _F.
    _E math:acos _C.
    (_F 2) math:exponentiation _Z12.
    (_Z12 1) math:difference _Z13.
    (_Z13 0.5) math:exponentiation _Z14.
    (_F _Z14) math:sum _Z15.
    (2.718281828459045 _U) math:exponentiation _Z15.
    _U math:negation _D.
