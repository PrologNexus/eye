@prefix schema: <http://schema.org/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

() log:onNegativeSurface {
    <> log:imports <../cha58/cha58-data.ttl>.
    () log:onNegativeSurface true.
}.

(skolem:S) log:onNegativeSurface {
    skolem:S a foaf:Person.
    () log:onNegativeSurface {
        skolem:S a schema:Person {| log:ohyeah {skolem:S a foaf:Person} |}.
    }.
}.

# query
(skolem:S skolem:C) log:onQuerySurface {
    skolem:S a schema:Person.
}.

(skolem:S skolem:A) log:onQuerySurface {
    skolem:S log:ohyeah skolem:A
}.
