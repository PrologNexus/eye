@prefix schema: <http://schema.org/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

() log:onNegativeSurface {
    <> log:imports <../cha58/cha58-data.ttl>.
    () log:onNegativeSurface true.
}.

(?S) log:onNegativeSurface {
    ?S a foaf:Person.
    () log:onNegativeSurface {
        ?S a schema:Person {| log:ohyeah {?S a foaf:Person} |}.
    }.
}.

# query
(?S ?C) log:onQuerySurface {
    ?S a schema:Person.
}.

(?S ?A) log:onQuerySurface {
    ?S log:ohyeah ?A
}.
