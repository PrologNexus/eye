@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

() log:onNegativeSurface {
    (?E) log:onNegativeSurface {
        ?E a :C.
    }.
}.

(?a ?b ?S1) log:onNegativeSurface {

    ?a log:onNegativeSurface {
        ?b log:onNegativeSurface ?S1.
    }.

    () log:onNegativeSurface {
        () log:onNegativeSurface ?S1.
    }.
}.

(?s ?p ?o) log:onQuerySurface {
    () log:onNegativeSurface {
        ?s ?p ?o.
    }.
}.
