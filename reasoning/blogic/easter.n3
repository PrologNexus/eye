# ----------
#
# See https://en.wikipedia.org/wiki/Date_of_Easter

@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix math: <http://www.w3.org/2000/10/swap/math#>.
@prefix : <http://example.org/ns#>.
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/d7882a29-44b8-4cbc-bc70-3bd11fba1fdb#>.

# calculate easter day
(
    skolem:x0 skolem:x1 skolem:x2 skolem:x3 skolem:x4 skolem:x5 skolem:x6 skolem:x7 skolem:x8 skolem:x9
    skolem:x10 skolem:x11 skolem:x12 skolem:x13 skolem:x14 skolem:x15 skolem:x16 skolem:x17 skolem:x18 skolem:x19
    skolem:x20 skolem:x21 skolem:x22 skolem:x23 skolem:x24 skolem:x25 skolem:x26 skolem:x27 skolem:x28 skolem:x29
    skolem:x30 skolem:x31 skolem:x32 skolem:x33 skolem:x34
) log:onNegativeSurface {
    () log:onNegativeSurface <<skolem:x0 :easter (skolem:x34 skolem:x32)>>.
    (skolem:x0 19) math:remainder skolem:x1.
    (skolem:x0 100) math:integerQuotient skolem:x2.
    (skolem:x0 100) math:remainder skolem:x3.
    (skolem:x2 4) math:integerQuotient skolem:x4.
    (skolem:x2 4) math:remainder skolem:x5.
    (skolem:x2 8) math:sum skolem:x6.
    (skolem:x6 25) math:integerQuotient skolem:x7.
    (skolem:x2 skolem:x7) math:difference skolem:x8.
    (skolem:x8 1) math:sum skolem:x9.
    (skolem:x9 3) math:integerQuotient skolem:x10.
    (19 skolem:x1) math:product skolem:x11.
    (skolem:x11 skolem:x2) math:sum skolem:x12.
    (skolem:x12 skolem:x4) math:difference skolem:x13.
    (skolem:x13 skolem:x10) math:difference skolem:x14.
    (skolem:x14 15) math:sum skolem:x15.
    (skolem:x15 30) math:remainder skolem:x16.
    (skolem:x3 4) math:integerQuotient skolem:x17.
    (skolem:x3 4) math:remainder skolem:x18.
    (2 skolem:x5) math:product skolem:x19.
    (2 skolem:x17) math:product skolem:x20.
    skolem:x16 math:negation skolem:x21.
    skolem:x18 math:negation skolem:x22.
    (32 skolem:x19 skolem:x20 skolem:x21 skolem:x22) math:sum skolem:x23.
    (skolem:x23 7) math:remainder skolem:x24.
    (11 skolem:x16) math:product skolem:x25.
    (22 skolem:x24) math:product skolem:x26.
    (skolem:x1 skolem:x25 skolem:x26) math:sum skolem:x27.
    (skolem:x27 451) math:integerQuotient skolem:x28.
    (7 skolem:x28) math:product skolem:x29.
    skolem:x29 math:negation skolem:x30.
    (skolem:x16 skolem:x24 skolem:x30 114) math:sum skolem:x31.
    (skolem:x31 31) math:integerQuotient skolem:x32.
    (skolem:x31 31) math:remainder skolem:x33.
    (skolem:x33 1) math:sum skolem:x34.
}.

# question
(skolem:x0 skolem:x1 skolem:x2 skolem:x3) log:onQuestionSurface {
    30 log:repeat skolem:x0.
    (2021 skolem:x0) math:sum skolem:x1.
    skolem:x1 :easter (skolem:x2 skolem:x3).
    () log:onAnswerSurface {
        skolem:x1 :easter (skolem:x2 skolem:x3).
    }.
}.
