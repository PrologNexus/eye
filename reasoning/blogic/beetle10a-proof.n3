@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix : <http://example.org/ns#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix var: <http://eyereasoner.github.io/var#>.
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:component skolem:lemma2;
    r:gives {
        :grannysmith :is :green.
        _:sk_47 :is :beautiful.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :grannysmith :is :green.
    };
    r:evidence (
        skolem:lemma3
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ n3:uri "http://example.org/ns#grannysmith"]];
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_1"]; r:boundTo [ n3:uri "http://example.org/ns#green"]];
    r:rule [ a r:DerivedQuery; r:gives {
        {
            var:x_0 :is var:x_1.
        } => {
            var:x_0 :is var:x_1.
        }.
    }].

skolem:lemma2 a r:Inference;
    r:gives {
        _:sk_47 :is :beautiful.
    };
    r:evidence (
        [ a r:Fact; r:gives {_:sk_47 :is :beautiful}]
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ a r:Existential; n3:nodeId "_:sk_47"]];
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_1"]; r:boundTo [ n3:uri "http://example.org/ns#beautiful"]];
    r:rule [ a r:DerivedQuery; r:gives {
        {
            var:x_0 :is var:x_1.
        } => {
            var:x_0 :is var:x_1.
        }.
    }].

skolem:lemma3 a r:Extraction;
    r:gives {
        :grannysmith :is :green.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/blogic/beetle10a.n3s>].

