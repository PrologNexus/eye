@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

(?S1 ?S2) log:onNegativeSurface {

    ?S1 log:equalTo {
        (?X ?Y) log:onNeutralSurface {
            :s1 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, ?X.
            :s2 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, ?Y.
            :s3 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s4 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s5 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s6 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s7 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s8 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s9 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s10 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
        } .
    } .

    ?S2 log:equalTo {
        (?Y ?X) log:onNeutralSurface {
            :s6 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s7 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s3 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s4 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s5 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s8 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s9 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s10 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s1 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, ?Y.
            :s2 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, ?X.
        } .
    } .

    ?S1 log:isomorphic ?S2 .

    () log:onNegativeSurface {
        :test :is true .
    } .
} .

() log:onQuerySurface {
    :test :is true .
} .
