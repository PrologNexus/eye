@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/d7882a29-44b8-4cbc-bc70-3bd11fba1fdb#>.

(skolem:S1 skolem:S2) log:onNegativeSurface {

    skolem:S1 log:equalTo {
        (skolem:X skolem:Y) log:onNeutralSurface {
            :s1 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, skolem:X.
            :s2 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, skolem:Y.
            :s3 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s4 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s5 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s6 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s7 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s8 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s9 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s10 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
        } .
    } .

    skolem:S2 log:equalTo {
        (skolem:Y skolem:X) log:onNeutralSurface {
            :s6 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s7 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s3 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s4 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s5 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s8 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s9 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s10 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, :o10.
            :s1 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, skolem:Y.
            :s2 :p :o1, :o2, :o3, :o4, :o5, :o6, :o7, :o8, :o9, skolem:X.
        } .
    } .

    skolem:S1 log:isomorphic skolem:S2 .

    () log:onNegativeSurface {
        :test :is true .
    } .
} .

() log:onQuerySurface {
    :test :is true .
} .
