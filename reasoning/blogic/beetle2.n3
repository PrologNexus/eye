@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

:beetle a :Car.

# all cars are red or green or blue
(_:A) log:onNegativeSurface {
    _:A a :Car.
    () log:onNegativeSurface {
        _:A :is :red.
    }.
    () log:onNegativeSurface {
        _:A :is :green.
    }.
    () log:onNegativeSurface {
        _:A :is :blue.
    }.
}.

# red things are beautiful
(_:A) log:onNegativeSurface {
    _:A :is :red.
    () log:onNegativeSurface {
        _:A :is :beautiful.
    }.
}.

# green things are beautiful
(_:A) log:onNegativeSurface {
    _:A :is :green.
    () log:onNegativeSurface {
        _:A :is :beautiful.
    }.
}.

# blue things are beautiful
(_:A) log:onNegativeSurface {
    _:A :is :blue.
    () log:onNegativeSurface {
        _:A :is :beautiful.
    }.
}.

# query
(_:S) log:onQuerySurface {
    _:S :is :beautiful.
}.
