@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix var: <http://eyereasoner.github.io/var#>.
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/3917894951695475063#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        :bob :saying :C.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :bob :saying :C.
    };
    r:evidence (
        skolem:lemma2
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ n3:uri "http://example.org/ns#bob"]];
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_1"]; r:boundTo [ n3:uri "http://example.org/ns#C"]];
    r:rule [ a r:DerivedQuery; r:gives {
        {
            var:x_0 :saying var:x_1.
        } => {
            var:x_0 :saying var:x_1.
        }.
    }].

skolem:lemma2 a r:Inference;
    r:gives {
        :bob :saying :C.
    };
    r:evidence (
        skolem:lemma3
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ n3:uri "http://example.org/ns#bob"]];
    r:rule [ a r:DerivedRule; r:gives {
        {
            var:x_0 a :Person.
        } => {
            var:x_0 :saying :C.
        }.
    }].

skolem:lemma3 a r:Extraction;
    r:gives {
        :bob a :Person.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/blogic/disj-elim.n3s>].

