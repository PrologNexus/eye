@prefix : <urn:example:> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .

(skolem:E) log:onNegativeSurface {
    skolem:E a :C .
} .

(skolem:a skolem:S1) log:onNegativeSurface {

    skolem:a log:onNegativeSurface skolem:S1.

    () log:onNegativeSurface {
        () log:onNeutralSurface skolem:S1.
    } .
} .

(skolem:S1) log:onQuerySurface {
    () log:onNeutralSurface skolem:S1.
} .
