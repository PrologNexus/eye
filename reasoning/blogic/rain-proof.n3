@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix var: <http://eyereasoner.github.io/var#>.
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/3917894951695475063#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:component skolem:lemma2;
    r:gives {
        :obs1 :ascribed :Rain.
        :obs1 :ascribed :WetGrass.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :obs1 :ascribed :Rain.
    };
    r:evidence (
        skolem:lemma3
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ n3:uri "http://example.org/ns#obs1"]];
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_1"]; r:boundTo [ n3:uri "http://example.org/ns#Rain"]];
    r:rule [ a r:DerivedQuery; r:gives {
        {
            var:x_0 :ascribed var:x_1.
        } => {
            var:x_0 :ascribed var:x_1.
        }.
    }].

skolem:lemma2 a r:Inference;
    r:gives {
        :obs1 :ascribed :WetGrass.
    };
    r:evidence (
        skolem:lemma4
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ n3:uri "http://example.org/ns#obs1"]];
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_1"]; r:boundTo [ n3:uri "http://example.org/ns#WetGrass"]];
    r:rule [ a r:DerivedQuery; r:gives {
        {
            var:x_0 :ascribed var:x_1.
        } => {
            var:x_0 :ascribed var:x_1.
        }.
    }].

skolem:lemma3 a r:Extraction;
    r:gives {
        :obs1 :ascribed :Rain.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/blogic/rain.n3s>].

skolem:lemma4 a r:Inference;
    r:gives {
        :obs1 :ascribed :WetGrass.
    };
    r:evidence (
        skolem:lemma5
        skolem:lemma3
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo [ n3:uri "http://example.org/ns#obs1"]];
    r:rule [ a r:DerivedRule; r:gives {
        {
            :currentWeather a :NormalWeather.
            var:x_0 :ascribed :Rain.
        } => {
            var:x_0 :ascribed :WetGrass.
        }.
    }].

skolem:lemma5 a r:Extraction;
    r:gives {
        :currentWeather a :NormalWeather.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/blogic/rain.n3s>].

