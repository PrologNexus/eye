@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix var: <http://eyereasoner.github.io/var#>.
@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        ((:s (:s 0)) (:s (:s (:s 0)))) :add (:s (:s (:s (:s (:s 0))))).
        ((:s (:s (:s (:s (:s 0))))) (:s (:s (:s (:s 0))))) :multiply (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s 0)))))))))))))))))))).
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        ((:s (:s 0)) (:s (:s (:s 0)))) :add (:s (:s (:s (:s (:s 0))))).
        ((:s (:s (:s (:s (:s 0))))) (:s (:s (:s (:s 0))))) :multiply (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s 0)))))))))))))))))))).
    };
    r:evidence (
        [ a r:Fact; r:gives {((:s (:s 0)) (:s (:s (:s 0)))) :add (:s (:s (:s (:s (:s 0)))))}]
        [ a r:Fact; r:gives {((:s (:s (:s (:s (:s 0))))) (:s (:s (:s (:s 0))))) :multiply (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s 0))))))))))))))))))))}]
    );
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_0"]; r:boundTo (:s (:s (:s (:s (:s 0)))))];
    r:binding [ r:variable [ n3:uri "http://eyereasoner.github.io/var#x_1"]; r:boundTo (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s (:s 0))))))))))))))))))))];
    r:rule [ a r:DerivedQuery; r:gives {
        {
            ((:s (:s 0)) (:s (:s (:s 0)))) :add var:x_0.
            (var:x_0 (:s (:s (:s (:s 0))))) :multiply var:x_1.
        } => {
            ((:s (:s 0)) (:s (:s (:s 0)))) :add var:x_0.
            (var:x_0 (:s (:s (:s (:s 0))))) :multiply var:x_1.
        }.
    }].

