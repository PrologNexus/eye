@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.

# all cars are nice or pretty or blue
(_:A) log:onNegativeSurface {
    _:A a :Car.
    () log:onNegativeSurface {
        _:A :is :nice11.
    }.
    () log:onNegativeSurface {
        _:A :is :nice12.
    }.
    () log:onNegativeSurface {
        _:A :is :nice21.
    }.
    () log:onNegativeSurface {
        _:A :is :nice22.
    }.
    () log:onNegativeSurface {
        _:A :is :pretty11.
    }.
    () log:onNegativeSurface {
        _:A :is :pretty12.
    }.
    () log:onNegativeSurface {
        _:A :is :pretty21.
    }.
    () log:onNegativeSurface {
        _:A :is :pretty22.
    }.
    () log:onNegativeSurface {
        _:A :is :blue.
    }.
}.

# pretty or nice or blue things are beautiful
(_:A) log:onNegativeSurface {
    () log:onNegativeSurface {
        () log:onNegativeSurface {
            _:A :is :pretty11.
        }.
        () log:onNegativeSurface {
            _:A :is :pretty12.
        }.
        () log:onNegativeSurface {
            _:A :is :pretty21.
        }.
        () log:onNegativeSurface {
            _:A :is :pretty22.
        }.
        () log:onNegativeSurface {
            _:A :is :nice11.
        }.
        () log:onNegativeSurface {
            _:A :is :nice12.
        }.
        () log:onNegativeSurface {
            _:A :is :nice21.
        }.
        () log:onNegativeSurface {
            _:A :is :nice22.
        }.
        () log:onNegativeSurface {
            _:A :is :blue.
        }.
    }.
    () log:onNegativeSurface {
        _:A :is :beautiful.
    }.
}.

# query
(_:S) log:onQuerySurface {
    _:S :is :beautiful.
}.
