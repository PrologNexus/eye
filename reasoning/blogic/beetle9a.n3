@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.

# bob is a person
:bob a :Person.

# green things are beautiful
(skolem:A) log:onNegativeSurface {
    skolem:A :is :green.
    () log:onNegativeSurface {
        skolem:A :is :beautiful.
    }.
}.

# things which are not green are beautiful
(skolem:A) log:onNegativeSurface {
    () log:onNegativeSurface {
        skolem:A :is :green
    }.
    () log:onNegativeSurface {
        skolem:A :is :beautiful.
    }.
}.

# query
(skolem:X) log:onQuerySurface {
    skolem:X :is :beautiful.
}.
