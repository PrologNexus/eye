#!/bin/bash
eye "$@" --wcache http://eyereasoner.github.io/eye/reasoning .. --nope http://eyereasoner.github.io/eye/reasoning/n3patch/sample-data.n3 http://eyereasoner.github.io/eye/reasoning/n3patch/patch-rule.n3 --pass --output patched-data.n3
eye "$@" --wcache http://eyereasoner.github.io/eye/reasoning .. --nope --blogic http://eyereasoner.github.io/eye/reasoning/n3patch/sample-data.n3 http://eyereasoner.github.io/eye/reasoning/n3patch/patch-surface.n3 --output patched-data-surface.n3
