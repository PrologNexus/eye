@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        :we :are :happy.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :we :are :happy.
    };
    r:evidence (
        skolem:lemma2
    );
    r:rule skolem:lemma3.

skolem:lemma2 a r:Inference;
    r:gives {
        :we :are :happy.
    };
    r:evidence (
        skolem:lemma4
        skolem:lemma5
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ a r:Existential; n3:nodeId "_:sk_0"]];
    r:rule skolem:lemma6.

skolem:lemma3 a r:Extraction;
    r:gives {
        {
            :we :are :happy.
        } => {
            :we :are :happy.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/ulr/universal_6_d.n3>].

skolem:lemma4 a r:Inference;
    r:gives {
        _:sk_0 :is :working.
    };
    r:evidence (
        [ a r:Fact; r:gives true]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ a r:Existential; n3:nodeId "_:sk_0"]];
    r:rule skolem:lemma7.

skolem:lemma5 a r:Inference;
    r:gives {
        :This :is :working.
    };
    r:evidence (
        [ a r:Fact; r:gives true]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#This"]];
    r:rule skolem:lemma7.

skolem:lemma6 a r:Extraction;
    r:gives {
        @forAll var:x_0. {
            var:x_0 :is :working.
            :This :is :working.
        } => {
            :we :are :happy.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/ulr/universal_6_d.n3>].

skolem:lemma7 a r:Extraction;
    r:gives {
        @forSome var:x_0. {
            var:x_0 :is :working.
        } <= true.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/ulr/universal_6_d.n3>].

