skolem:lemma13 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1, var:x_2, var:x_3. {
            var:x_0 => var:x_1.
            var:x_1 list:select (var:x_2 var:x_3).
            var:x_2 => ().
        } => {
            var:x_0 => var:x_3.
        }.
    };
    r:because [ a r:Parsing; r:source <file:///home/jdroo/github.com/eyereasoner/eye/reasoning/ulr/ulr.n3>].

skolem:lemma14 a r:Extraction;
    r:gives {
        @forAll var:x_0. {
            var:x_0 a :Car.
        } => {
            {
                var:x_0 :is :beautiful.
            } => ().
        }.
    };
    r:because [ a r:Parsing; r:source <file:///home/jdroo/github.com/eyereasoner/eye/reasoning/ulr/beetle6.n3>].

