@prefix graph: <http://www.w3.org/2000/10/swap/graph#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix : <#>.

# graph statement
_:cgs_1 {
    :u :v :w.
    :x :y :z.
}

# void statement
_:cgs_1 graph:statement _:cgs_1.

# forward rule
_:cgs_2 log:implies _:cgs_3.

_:cgs_2 {
    var:X graph:statement _:cgs_1.
}

_:cgs_3 {
    :test :is true.
}

# query
_:cgs_3 log:query _:cgs_3.

_:cgs_4 log:query _:cgs_4.

_:cgs_4 {
    var:G graph:statement var:C.
}
