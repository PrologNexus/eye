@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix time: <http://www.w3.org/2000/10/swap/time#>.
@prefix math: <http://www.w3.org/2000/10/swap/math#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <https://example.org/#>.

:patH :birthDay "1944-08-21"^^xsd:date.
:josD :birthDay "1956-01-10"^^xsd:date.
:vicR :birthDay "2015-10-26"^^xsd:date.

(_:S _:A _:B _:D _:F) log:onNegativeSurface {
    () log:onNegativeQuestionSurface {
        _:S :ageAbove _:A.
    }.
    _:S :birthDay _:B.
    "" time:localTime _:D.
    (_:D _:B) math:difference _:F.
    _:F math:greaterThan _:A.
}.

(_:S) log:onNegativeSurface {
    _:S :ageAbove "P18Y"^^xsd:duration.
    () log:onNegativeAnswerSurface {
        _:S :ageAbove "P18Y"^^xsd:duration.
    }.
}.
