@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix func: <http://www.w3.org/2007/rif-builtin-function#>.
@prefix time: <http://www.w3.org/2000/10/swap/time#>.
@prefix math: <http://www.w3.org/2000/10/swap/math#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <https://example.org/#>.

:patH :birthDay "1944-08-21"^^xsd:date.
:josD :birthDay "1956-01-10"^^xsd:date.

(_:S _:A _:B _:D _:YB _:YD _:AD) log:onNegativeSurface {
    () log:onNegativeQuestionSurface {
        _:S :ageAbove _:A.
    }.
    _:S :birthDay _:B.
    "" time:localTime _:D.
    (_:B) func:year-from-date _:YB.
    (_:D) func:year-from-dateTime _:YD.
    (_:YD _:YB) math:difference _:AD.
    _:AD math:greaterThan _:A.
}.

(_:S) log:onNegativeSurface {
    _:S :ageAbove 75.
    () log:onNegativeAnswerSurface {
        _:S :ageAbove 75.
    }.
}.
