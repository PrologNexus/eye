@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:component skolem:lemma2;
    r:gives {
        {
            log:complement log:complement log:complement.
            log:complement log:complement log:complement.
            log:complement log:complement log:complement.
        } => false.
        @forAll var:P, var:C, var:S, var:O. {
            var:P log:complement var:C.
            var:S var:P var:O.
            var:S var:C var:O.
        } => false.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        {
            log:complement log:complement log:complement.
            log:complement log:complement log:complement.
            log:complement log:complement log:complement.
        } => false.
    };
    r:evidence (
        [ a r:Fact; r:gives {{
            log:complement log:complement log:complement.
            log:complement log:complement log:complement.
            log:complement log:complement log:complement.
        } => false}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://www.w3.org/2000/10/swap/log#complement"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ n3:uri "http://www.w3.org/2000/10/swap/log#complement"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_2"]; r:boundTo [ n3:uri "http://www.w3.org/2000/10/swap/log#complement"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_3"]; r:boundTo [ n3:uri "http://www.w3.org/2000/10/swap/log#complement"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0, var:x_1, var:x_2, var:x_3. {
        {
            var:x_0 log:complement var:x_1.
            var:x_2 var:x_0 var:x_3.
            var:x_2 var:x_1 var:x_3.
        } => false.
    } => {
        {
            var:x_0 log:complement var:x_1.
            var:x_2 var:x_0 var:x_3.
            var:x_2 var:x_1 var:x_3.
        } => false.
    }}].

skolem:lemma2 a r:Inference;
    r:gives {
        @forAll var:P, var:C, var:S, var:O. {
            var:P log:complement var:C.
            var:S var:P var:O.
            var:S var:C var:O.
        } => false.
    };
    r:evidence (
        [ a r:Fact; r:gives {@forSome var:P, var:C, var:S, var:O. {
            var:P log:complement var:C.
            var:S var:P var:O.
            var:S var:C var:O.
        } => false}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://www.w3.org/2000/10/swap/var#P"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ n3:uri "http://www.w3.org/2000/10/swap/var#C"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_2"]; r:boundTo [ n3:uri "http://www.w3.org/2000/10/swap/var#S"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_3"]; r:boundTo [ n3:uri "http://www.w3.org/2000/10/swap/var#O"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0, var:x_1, var:x_2, var:x_3. {
        {
            var:x_0 log:complement var:x_1.
            var:x_2 var:x_0 var:x_3.
            var:x_2 var:x_1 var:x_3.
        } => false.
    } => {
        {
            var:x_0 log:complement var:x_1.
            var:x_2 var:x_0 var:x_3.
            var:x_2 var:x_1 var:x_3.
        } => false.
    }}].

