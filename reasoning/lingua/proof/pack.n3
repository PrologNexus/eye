@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix vcard: <http://www.w3.org/2006/vcard/ns#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix pack: <https://example.org/ns/package#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix sign: <https://example.org/ns/signature#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        <https://example.org/ns/example#Bob> vcard:bday "2000-01-01T09:00:00.000Z"^^xsd:dateTime.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        <https://example.org/ns/example#Bob> vcard:bday "2000-01-01T09:00:00.000Z"^^xsd:dateTime.
    };
    r:evidence (
        [ a r:Fact; r:gives {skolem:node_1 pack:package skolem:bng_1}]
        [ a r:Fact; r:gives {skolem:bng_1 log:includes skolem:bng_2}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_3"]; r:boundTo skolem:bng_3];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ a r:Existential; n3:nodeId "https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#node_1"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo skolem:bng_1];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_2"]; r:boundTo [ a r:Existential; n3:nodeId "https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#node_3"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0, var:x_1, var:x_2, var:x_3. skolem:bng_4 log:implies var:x_3}].


skolem:bng_1 {
    skolem:node_3 pack:content skolem:bng_3.
    skolem:node_3 pack:origin <https://example.org/ns/example#Endpoint>.
    skolem:node_3 pack:createdAt "2024-01-08T17:08:52.165Z"^^xsd:dateTime.
    skolem:node_5 a sign:Signature.
    skolem:node_5 sign:issuer <https://example.org/ns/example#Alice>.
    skolem:node_5 sign:created "2024-01-08T17:08:52.166Z"^^xsd:dateTime.
    skolem:node_5 sign:proofValue "sSJ0xHT7yH2MeYjI6I7fVy+PRfh/EDJkTEOhbCA2BYcd+GBJRD1BQV1rwVe69cNPHhtvGKbITIf7TBlbpkE6YANMNNS2aSQMw8i6TLTXa16zhukp+V1nLYKE/51rt/Us".
    skolem:node_3 pack:hasContentSignature skolem:node_5.
}

skolem:bng_2 {
    skolem:node_3 pack:content skolem:bng_3.
}

skolem:bng_3 {
    <https://example.org/ns/example#Bob> vcard:bday "2000-01-01T09:00:00.000Z"^^xsd:dateTime.
}

skolem:bng_4 {
    var:x_0 pack:package var:x_1.
    var:x_1 log:includes skolem:bng_5.
}

skolem:bng_5 {
    var:x_2 pack:content var:x_3.
}
