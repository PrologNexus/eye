@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://eyereasoner.github.io/eye/reasoning/lingua/acp.trig#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        :test :for :PolicyX.
        :test :is true.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :test :for :PolicyX.
        :test :is true.
    };
    r:evidence (
        skolem:lemma2
        [ a r:Fact; r:gives {:PolicyX :pass :allOfTest}]
        [ a r:Fact; r:gives {:PolicyX :pass :anyOfTest}]
        [ a r:Fact; r:gives {:PolicyX :pass :noneOfTest}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://eyereasoner.github.io/eye/reasoning/lingua/acp.trig#PolicyX"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0. skolem:bng_1 log:implies skolem:bng_2}].

skolem:lemma2 a r:Extraction;
    r:gives {
        :PolicyX a :Policy.
    };
    r:because [ a r:Parsing; r:source <http://eyereasoner.github.io/eye/reasoning/lingua/acp.trig>].


skolem:bng_1 {
    var:x_0 a :Policy.
    var:x_0 :pass :allOfTest.
    var:x_0 :pass :anyOfTest.
    var:x_0 :pass :noneOfTest.
}

skolem:bng_2 {
    :test :for var:x_0.
    :test :is true.
}
