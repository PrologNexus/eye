@prefix skolem: <http://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://eyereasoner.github.io/eye/reasoning/lingua/union.trig#>.
@prefix graph: <http://www.w3.org/2000/10/swap/graph#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        :result :is skolem:bng_1.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :result :is skolem:bng_1.
    };
    r:evidence (
        [ a r:Fact; r:gives {(skolem:bng_2 skolem:bng_3) graph:union skolem:bng_1}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo skolem:bng_1];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0. skolem:bng_4 log:implies skolem:bng_5}].


skolem:bng_1 {
    :A :B :C.
    :A :X :C.
    :D :E :F.
    :A :Y :C.
}

skolem:bng_2 {
    :A :B :C.
    :A :X :C.
    :D :E :F.
}

skolem:bng_3 {
    :A :Y :C.
    :A :B :C.
}

skolem:bng_4 {
    (skolem:bng_2 skolem:bng_3) graph:union var:x_0.
}

skolem:bng_5 {
    :result :is var:x_0.
}

skolem:bng_2 {
    :A :B :C.
    :A :X :C.
    :D :E :F.
}

skolem:bng_3 {
    :A :Y :C.
    :A :B :C.
}
