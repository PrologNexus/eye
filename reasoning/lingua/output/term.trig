@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

_:node_1 graph:term _:node_1.
_:node_1 graph:member _:bng_1.
_:node_4 graph:term _:node_4.
_:node_4 graph:member _:node_4.

_:node_1 {
    var:A rdfs:subClassOf var:B.
    var:S a var:A.
}

_:bng_1 {
    var:A rdfs:subClassOf var:B.
}

_:node_4 {
    var:X rdfs:subClassOf var:Y.
}
