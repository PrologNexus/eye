@prefix : <http://example.org/#>.

# i element of A
:i a :A.

# not i element of C
{
    :i a :C.
} => ($ $).

# not not not i element of D
{
    {
        {
            :i a :D.
        } => ($ $).
    } => ($ $).
} => ($ $).

# all A are B or C or D
{
    ?S a :A.
} => ($ {
    ?S a :B.
} {
    ?S a :C.
} {
    ?S a :D.
} $).

# query
($ $) <= {
    ?S a ?C.
}.
