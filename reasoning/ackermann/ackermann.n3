@prefix math: <http://www.w3.org/2000/10/swap/math#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <urn:example:>.

{
    (0 ?Y) :ackermann ?A.
} <= {
    (?Y 1) math:sum ?A.
}.

{
    (1 ?Y) :ackermann ?A.
} <= {
    (?Y 2) math:sum ?A.
}.

{
    (2 ?Y) :ackermann ?A.
} <= {
    (2 ?Y) math:product ?B.
    (?B 3) math:sum ?A.
}.

{
    (3 ?Y) :ackermann ?A.
} <= {
    (?Y 3) math:sum ?B.
    (2 ?B) math:exponentiation ?C.
    (?C 3) math:difference ?A.
}.

{
    (4 0) :ackermann ?A.
} <= {
    (2 1) math:exponentiation ?C.
    (2 ?C) math:exponentiation ?D.
    (2 ?D) math:exponentiation ?E.
    (?E 3) math:difference ?A.
}.

{
    (4 1) :ackermann ?A.
} <= {
    (2 1) math:exponentiation ?C.
    (2 ?C) math:exponentiation ?D.
    (2 ?D) math:exponentiation ?E.
    (2 ?E) math:exponentiation ?F.
    (?F 3) math:difference ?A.
}.

{
    (4 2) :ackermann ?A.
} <= {
    (2 1) math:exponentiation ?C.
    (2 ?C) math:exponentiation ?D.
    (2 ?D) math:exponentiation ?E.
    (2 ?E) math:exponentiation ?F.
    (2 ?F) math:exponentiation ?G.
    (?G 3) math:difference ?A.
}.

