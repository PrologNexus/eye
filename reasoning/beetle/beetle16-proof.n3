@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        :beetle :is :old.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :beetle :is :old.
    };
    r:evidence (
        skolem:lemma2
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ n3:uri "http://example.org/#old"]];
    r:rule skolem:lemma3.

skolem:lemma2 a r:Inference;
    r:gives {
        :beetle :is :old.
    };
    r:evidence (
        skolem:lemma4
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:rule skolem:lemma5.

skolem:lemma3 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1. {
            var:x_0 :is var:x_1.
        } => {
            var:x_0 :is var:x_1.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/beetle/beetle16.n3>].

skolem:lemma4 a r:Inference;
    r:gives {
        {
            :beetle :is :new.
        } log:not true.
    };
    r:evidence (
        skolem:lemma6
        skolem:lemma7
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:rule skolem:lemma8.

skolem:lemma5 a r:Extraction;
    r:gives {
        @forAll var:x_0. {
            {
                var:x_0 :is :new.
            } log:not true.
        } => {
            var:x_0 :is :old.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/beetle/beetle16.n3>].

skolem:lemma6 a r:Extraction;
    r:gives {
        :beetle a :Car.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/beetle/beetle16.n3>].

skolem:lemma7 a r:Extraction;
    r:gives {
        {
            :beetle :is :beautiful.
        } log:not true.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/beetle/beetle16.n3>].

skolem:lemma8 a r:Extraction;
    r:gives {
        @forAll var:x_0. {
            var:x_0 a :Car.
            {
                var:x_0 :is :beautiful.
            } log:not true.
        } => {
            {
                var:x_0 :is :new.
            } log:not true.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/beetle/beetle16.n3>].

