@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/#>.

{
    <> log:imports <https://eyereasoner.github.io/eye/reasoning/semifol/semifol-resolution.n3>.
    <> log:imports <https://eyereasoner.github.io/eye/reasoning/semifol/semifol-fuse.n3>.
} => true.

# beetle is a car
:beetle a :Car.

# beetle is not beautiful
{
    :beetle :is :beautiful.
} log:not true.

# if something is a car and not beautiful it is not new
{
    ?A a :Car.
    {
        ?A :is :beautiful.
    } log:not true.
} => {
    {
        ?A :is :new.
    } log:not true.
}.

# if something is not new it is old
{
    {
        ?A :is :new.
    } log:not true.
} => {
    ?A :is :old.
}.

