@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        :beetle :is :beautiful.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :beetle :is :beautiful.
    };
    r:evidence (
        skolem:lemma2
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ n3:uri "http://example.org/#beautiful"]];
    r:rule skolem:lemma3.

skolem:lemma2 a r:Inference;
    r:gives {
        :beetle :is :beautiful.
    };
    r:evidence (
        skolem:lemma4
        [ a r:Fact; r:gives {{
            :beetle :is :green.
        } => {
            :beetle :is :beautiful.
        }}]
        [ a r:Fact; r:gives {{
            :beetle :is :blue.
        } => {
            :beetle :is :beautiful.
        }}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_2"]; r:boundTo {
        :beetle :is :beautiful.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo {
        :beetle :is :green.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo {
        :beetle :is :blue.
    }];
    r:rule skolem:lemma5.

skolem:lemma3 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1. {
            var:x_0 :is var:x_1.
        } => {
            var:x_0 :is var:x_1.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/beetle/beetle.n3>].

skolem:lemma4 a r:Inference;
    r:gives {
        {
            :beetle :is :green.
        } :or {
            :beetle :is :blue.
        }.
    };
    r:evidence (
        skolem:lemma6
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:rule skolem:lemma7.

skolem:lemma5 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1, var:x_2. {
            var:x_0 :or var:x_1.
            var:x_0 => var:x_2.
            var:x_1 => var:x_2.
        } => var:x_2.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/beetle/beetle.n3>].

skolem:lemma6 a r:Extraction;
    r:gives {
        :beetle a :Car.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/beetle/beetle.n3>].

skolem:lemma7 a r:Extraction;
    r:gives {
        @forAll var:x_0. {
            var:x_0 a :Car.
        } => {
            {
                var:x_0 :is :green.
            } :or {
                var:x_0 :is :blue.
            }.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/beetle/beetle.n3>].

