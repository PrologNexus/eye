@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/#>.

# beetle is a car
:beetle a :Car.

# all cars are green or blue
{
    ?S a :Car.
} => ($
    {
        ?S :is :green.
    } {
        ?S :is :blue.
    }
$).

# green things are beautiful
{
    ?S :is :green.
} => {
    ?S :is :beautiful.
}.

# blue things are beautiful
{
    ?S :is :blue.
} => {
    ?S :is :beautiful.
}.

# resolution
{
    ?P => ($ ?A ?B $).
    ?A => ?C.
} => {
    ?P => ($ ?C ?B $).
}.

{
    ?P => ($ ?A ?B $).
    ?B => ?C.
} => {
    ?P => ($ ?A ?C $).
}.

# factoring
{
    ?P => ($ ?A ?B $).
    ?A log:isomorphic ?B.
} => {
    ?P => ?A.
}.

# query
{
    ?S :is :beautiful.
} log:query {
    ?S :is :beautiful.
}.
