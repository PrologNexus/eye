@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/#>.

# beetle is a car
:beetle a :Car.

# all cars are green or not blue
{
    ?A a :Car.
} => {
    {
        ?A :is :green.
    } log:or {
        {
            ?A :is :blue.
        } log:not true.
    }.
}.

# green things are beautiful
{
    ?A :is :green.
} => {
    ?A :is :beautiful.
}.

# things which are not blue are beautiful
{
    {
        ?A :is :blue.
    } log:not true.
} => {
    ?A :is :beautiful.
}.

