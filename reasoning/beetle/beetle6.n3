@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/#>.

# beetle is a car
:beetle a :Car.

# all cars are green or blue
{
    ?A a :Car.
} log:sequent ({
    ?A :is :green.
} {
    ?A :is :blue.
}).

# green things are nice or pretty
{
    ?A :is :green.
} log:sequent ({
    ?A :is :nice.
} {
    ?A :is :pretty.
}).

# pretty things are beautiful
{
    ?A :is :pretty.
} => {
    ?A :is :beautiful.
}.

# cars are not beautiful
{
    ?A a :Car.
} => {
    {
        ?A :is :beautiful.
    } log:sequent ().
}.

# blue things are beautiful
{
    ?A :is :blue.
} => {
    ?A :is :beautiful.
}.

# query
{
    ?S :is ?O.
} =^ {
    ?S :is ?O.
}.
