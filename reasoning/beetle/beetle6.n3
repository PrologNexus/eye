@prefix : <http://example.org/#>.

# beetle is a car
:beetle a :Car.

# all cars are green or blue
{
    ?A a :Car.
} => {
    {
        ?A :is :green.
    } :or {
        ?A :is :blue.
    }.
}.

# green things are nice or pretty
{
    ?A :is :green.
} => {
    {
        ?A :is :nice.
    } :or {
        ?A :is :pretty.
    }.
}.

# pretty things are beautiful
{
    ?A :is :pretty.
} => {
    ?A :is :beautiful.
}.

# cars are not beautiful
{
    ?A a :Car.
} => {
    {
        ?A :is :beautiful.
    } :not true.
}.

# blue things are beautiful
{
    ?A :is :blue.
} => {
    ?A :is :beautiful.
}.

# query
{
    ?S :is ?O.
} |> {
    ?S :is ?O.
}.
