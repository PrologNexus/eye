@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:component skolem:lemma2;
    r:gives {
        :pers001 :familyName "Garcia".
        :pers001 :givenName "Alex".
        :pers002 :familyName "Cardinale".
        :pers002 :givenName "Claudia".
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :pers001 :familyName "Garcia".
        :pers001 :givenName "Alex".
    };
    r:evidence (
        skolem:lemma3
        [ a r:Fact; r:gives {:pers001 :givenName "Alex"}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#pers001"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo "Garcia"];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_2"]; r:boundTo "Alex"];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0, var:x_1, var:x_2. {
        var:x_0 :familyName var:x_1.
        var:x_0 :givenName var:x_2.
    } => {
        var:x_0 :familyName var:x_1.
        var:x_0 :givenName var:x_2.
    }}].

skolem:lemma2 a r:Inference;
    r:gives {
        :pers002 :familyName "Cardinale".
        :pers002 :givenName "Claudia".
    };
    r:evidence (
        skolem:lemma4
        skolem:lemma5
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#pers002"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo "Cardinale"];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_2"]; r:boundTo "Claudia"];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0, var:x_1, var:x_2. {
        var:x_0 :familyName var:x_1.
        var:x_0 :givenName var:x_2.
    } => {
        var:x_0 :familyName var:x_1.
        var:x_0 :givenName var:x_2.
    }}].

skolem:lemma3 a r:Extraction;
    r:gives {
        :pers001 :familyName "Garcia".
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/holographs/patch.trig>].

skolem:lemma4 a r:Extraction;
    r:gives {
        :pers002 :familyName "Cardinale".
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/holographs/patch.trig>].

skolem:lemma5 a r:Extraction;
    r:gives {
        :pers002 :givenName "Claudia".
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/holographs/patch.trig>].

