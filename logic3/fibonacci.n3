@prefix math: <http://www.w3.org/2000/10/swap/math#>.
@prefix : <http://eyereasoner.github.io/eye/reasoning#>.

{
    ?X :fibonacci ?Y.
} <= {
    (?X 0 1) :fib ?Y.
}.

{
    (0 ?A ?B) :fib ?A.
} <= true.

{
    (1 ?A ?B) :fib ?B.
} <= true.

{
    (?X ?A ?B) :fib ?Y.
} <= {
    ?X math:greaterThan 1.
    (?X 1) math:difference ?C.
    (?A ?B) math:sum ?D.
    (?C ?B ?D) :fib ?Y.
}.

# test
{
    0 :fibonacci ?F1.
    1 :fibonacci ?F2.
    6 :fibonacci ?F3.
    91 :fibonacci ?F4.
    283 :fibonacci ?F5.
    3674 :fibonacci ?F6.
} => {
    :test :is {
        0 :fibonacci ?F1.
        1 :fibonacci ?F2.
        6 :fibonacci ?F3.
        91 :fibonacci ?F4.
        283 :fibonacci ?F5.
        3674 :fibonacci ?F6.
    }.
}.
