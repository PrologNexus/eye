#!/bin/bash
eye --quiet --skolem-genid 8b98b360-9a70-4845-b52c-c675af60ad01 --wcache https://eyereasoner.github.io/eye/color .. --nope https://eyereasoner.github.io/eye/color/socrates-star/socrates.n3 --query https://eyereasoner.github.io/eye/color/socrates-star/socrates-query.n3 --output socrates-answer.n3
eye --quiet --skolem-genid 8b98b360-9a70-4845-b52c-c675af60ad01 --wcache https://eyereasoner.github.io/eye/color .. https://eyereasoner.github.io/eye/color/socrates-star/socrates.n3 --query https://eyereasoner.github.io/eye/color/socrates-star/socrates-query.n3 --output socrates-proof.n3
eye --quiet --skolem-genid 8b98b360-9a70-4845-b52c-c675af60ad01 --wcache https://eyereasoner.github.io/eye/color .. --nope --proof https://eyereasoner.github.io/eye/color/socrates-star/socrates-proof.n3 --pass-only-new --output socrates-lemma.n3
