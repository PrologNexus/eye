@prefix graph: <http://www.w3.org/2000/10/swap/graph#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix rule: <http://eyereasoner.github.io/rule#>.
@prefix : <http://example.org/ns#>.

:Let :param1 ( 
    (:A :B :C)
    (:A :X :C) 
    (:D :E :F)
).

:Let :param2 (
    (:A :Y :C)
    (:A :B :C)
).

[] a rule:QueryRule;
    rule:premise (
        (:Let :param1 _:X1)
        (:Let :param2 _:X2)
        ((_:X1 _:X2) graph:union _:Y)
    );
    rule:conclusion (
        ((_:X1 _:X2) :union _:Y)
).
