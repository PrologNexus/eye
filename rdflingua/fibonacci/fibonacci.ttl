@prefix math: <http://www.w3.org/2000/10/swap/math#>.
@prefix rule: <http://eyereasoner.github.io/rule#>.
@prefix : <http://example.org/ns#>.

# backward rules
[] a rule:BackwardRule;
    rule:graffiti (
        _:X
        _:Y
    );
    rule:conclusion (
        (_:X :fibonacci _:Y)
    );
    rule:premise (
        ((_:X 0 1) :fib _:Y)
    ).

[] a rule:BackwardRule;
    rule:graffiti (
        _:A
        _:B
    );
    rule:conclusion (
        ((0 _:A _:B) :fib _:A)
    );
    rule:premise ().

[] a rule:BackwardRule;
    rule:graffiti (
        _:A
        _:B
    );
    rule:conclusion (
        ((1 _:A _:B) :fib _:B)
    );
    rule:premise ().

[] a rule:BackwardRule;
    rule:graffiti (
        _:X
        _:A
        _:B
        _:Y
        _:C
        _:D
    );
    rule:conclusion (
        ((_:X _:A _:B) :fib _:Y)
    );
    rule:premise (
        (_:X math:greaterThan 1)
        ((_:X 1) math:difference _:C)
        ((_:A _:B) math:sum _:D)
        ((_:C _:B _:D) :fib _:Y)
    ).

