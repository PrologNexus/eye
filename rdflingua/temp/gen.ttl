@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rule: <http://www.w3.org/2000/10/swap/rule#>.
@prefix : <http://example.org/ns#>.

:gen_rule a rule:ForwardRule;
    rule:vars ();
    rule:premise ();
    rule:conclusion (
        (:Socrates rdf:type :Human)
        (:Human rdfs:subClassOf :Mortal)
        (:rdfs_subclass_rule rdf:type rule:ForwardRule)
        (:rdfs_subclass_rule rule:vars (
            _:A
            _:B
            _:S
        ))
        (:rdfs_subclass_rule rule:premise (
            (_:A rdfs:subClassOf _:B)
            (_:S rdf:type _:A)
        ))
        (:rdfs_subclass_rule rule:conclusion (
            (_:S rdf:type _:B)
        ))
        (:socrates_query rdf:type rule:QueryRule)
        (:socrates_query rule:vars (
            _:S
        ))
        (:socrates_query rule:premise (
            (_:S rdf:type :Mortal)
        ))
        (:socrates_query rule:conclusion (
            (_:S rdf:type :Mortal)
        ))
    ).
