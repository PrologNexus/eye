:main_rule lingua:bindings ((var:cat :Alice) (var:string "http://example.org/ns#Alice") (var:string2 "http://example.org/ns#Alice_derived_rule") (var:name :Alice_derived_rule) (var:dog var:x_0)).
:main_rule lingua:bindings ((var:cat :Minka) (var:string "http://example.org/ns#Minka") (var:string2 "http://example.org/ns#Minka_derived_rule") (var:name :Minka_derived_rule) (var:dog var:x_0)).
