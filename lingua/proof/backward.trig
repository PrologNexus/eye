@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix math: <http://www.w3.org/2000/10/swap/math#>.

:backward_rule lingua:body term:backward_rule_body.
:backward_rule lingua:head term:backward_rule_head.
:backward_rule lingua:bindings ((var:X 5) (var:Y 3)).
:query lingua:question term:question.
:query lingua:answer term:question.
:query lingua:bindings ().
("this" 5) :moreInterestingThan ("that" 3).
term:backward_rule_body {
    var:X math:greaterThan var:Y.
}
term:backward_rule_head {
    ("this" var:X) :moreInterestingThan ("that" var:Y).
}
term:question {
    ("this" 5) :moreInterestingThan ("that" 3).
}
