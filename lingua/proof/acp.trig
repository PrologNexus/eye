:acp_rule1 lingua:body term:acp_rule1_body.
:acp_rule1 lingua:head term:acp_rule1_head.
:acp_rule1 lingua:bindings ((var:Test :test1) (var:Pol :PolicyX) (var:Field var:U_0) (var:X ((<https://eyereasoner.github.io/eye/lingua/acp.trig>) 1))).
:acp_rule2 lingua:body term:acp_rule2_body.
:acp_rule2 lingua:head term:acp_rule2_head.
:acp_rule2 lingua:bindings ((var:Test :test1) (var:Pol :PolicyX) (var:Field var:U_1) (var:List (:C)) (var:X ((<https://eyereasoner.github.io/eye/lingua/acp.trig>) 1)) (var:L 1)).
:acp_rule3 lingua:body term:acp_rule3_body.
:acp_rule3 lingua:head term:acp_rule3_head.
:acp_rule3 lingua:bindings ((var:Test :test1) (var:Pol :PolicyX) (var:Field var:U_2) (var:List ()) (var:X ((<https://eyereasoner.github.io/eye/lingua/acp.trig>) 1)) (var:L 0)).
:acp_query lingua:question term:acp_question.
:acp_query lingua:answer term:acp_answer.
:acp_query lingua:bindings ((var:Pol :PolicyX)).
:test :for :PolicyX.
:test :is true.
term:acp_rule1_body {
    var:Test :policy var:Pol.
    var:Pol a :Policy.
    (term:acp_rule1_fa1 term:acp_rule1_fa2) log:forAllIn var:X.
}
term:acp_rule1_head {
    var:Pol :pass :allOfTest.
}
term:acp_rule2_body {
    var:Test :policy var:Pol.
    var:Pol a :Policy.
    (var:Field term:acp_rule2_where var:List) log:collectAllIn var:X.
    var:List list:length var:L.
    var:L log:notEqualTo 0.
}
term:acp_rule2_head {
    var:Pol :pass :anyOfTest.
}
term:acp_rule3_body {
    var:Test :policy var:Pol.
    var:Pol a :Policy.
    (var:Field term:acp_rule3_where var:List) log:collectAllIn var:X.
    var:List list:length var:L.
    var:L log:equalTo 0.
}
term:acp_rule3_head {
    var:Pol :pass :noneOfTest.
}
term:acp_question {
    var:Pol a :Policy.
    var:Pol :pass :allOfTest.
    var:Pol :pass :anyOfTest.
    var:Pol :pass :noneOfTest.
}
term:acp_answer {
    :test :for var:Pol.
    :test :is true.
}
term:acp_rule1_fa1 {
    var:Pol :allOf var:Field.
}
term:acp_rule1_fa2 {
    var:Test :has var:Field.
}
term:acp_rule2_where {
    var:Pol :anyOf var:Field.
    var:Test :has var:Field.
}
term:acp_rule3_where {
    var:Pol :noneOf var:Field.
    var:Test :has var:Field.
}
