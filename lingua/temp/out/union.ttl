:union_query lingua:bindings ((:A :B :C :A :X :C :D :E :F) (:A :Y :C :A :B :C) (
    :A :B :C
    :A :X :C
    :D :E :F
    :A :Y :C
)).
((:A :B :C :A :X :C :D :E :F) (:A :Y :C :A :B :C)) :union (
    :A :B :C
    :A :X :C
    :D :E :F
    :A :Y :C
).
