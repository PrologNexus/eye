@prefix list: <http://www.w3.org/2000/10/swap/list#>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix lingua: <http://www.w3.org/2000/10/swap/lingua#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix : <http://example.org/ns#>.

:eats_rule a lingua:ForwardRule;
    lingua:premise (lingua:conjunction
        var:L log:equalTo ((var:A1 :norwegian var:A2 var:A3 var:A4) (:blue var:A5 var:A6 var:A7 var:A8) (var:A9 var:A10 var:A11 :milk var:A12) var:A13 var:A14)
        var:L :pair ((var:A15 var:A16 var:A17 var:A18 :blends) (var:A19 var:A20 :cats var:A21 var:A22))
        var:L :pair ((var:A23 var:A24 :horse var:A25 var:A26) (var:A27 var:A28 var:A29 var:A30 :dunhill))
        var:L :sublist ((:green var:A31 var:A32 :coffee var:A33) (:white var:A34 var:A35 var:A36 var:A37))
        var:L :pair ((var:A38 var:A39 var:A40 var:A41 :blends) (var:A42 var:A43 var:A44 :water var:A45))
        var:L list:member (:red :brit var:A46 var:A47 var:A48)
        var:L list:member (var:A49 :swede :dogs var:A50 var:A51)
        var:L list:member (var:A52 :dane var:A53 :tea var:A54)
        var:L list:member (var:A55 var:A56 :birds var:A57 :pallmall)
        var:L list:member (:yellow var:A58 var:A59 var:A60 :dunhill)
        var:L list:member (var:A61 var:A62 var:A63 :beer :bluemasters)
        var:L list:member (var:A64 :german var:A65 var:A66 :prince)
        var:L list:member (var:A67 var:B :fish var:A69 var:A70)
    );
    lingua:conclusion (lingua:conjunction
        var:B :eats :fish
    ).

:pair_rule1 a lingua:BackwardRule;
    lingua:conclusion (lingua:conjunction
        var:A :pair (var:B var:C)
    );
    lingua:premise (lingua:conjunction
        var:A :sublist (var:B var:C)
    ).

:pair_rule2 a lingua:BackwardRule;
    lingua:conclusion (lingua:conjunction
        var:A :pair (var:B var:C)
    );
    lingua:premise (lingua:conjunction
        var:A :sublist (var:C var:B)
    ).

:sublist_rule a lingua:BackwardRule;
    lingua:conclusion (lingua:conjunction
        var:A :sublist var:B
    );
    lingua:premise (lingua:conjunction
        (var:C var:D) list:append var:A
        (var:E var:B) list:append var:C
    ).

# query
:query_rule a lingua:QueryRule;
    lingua:premise (lingua:conjunction
        var:WHO :eats :fish
    );
    lingua:conclusion (lingua:conjunction
        var:WHO :eats :fish
    ).
