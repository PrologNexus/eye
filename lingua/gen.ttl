@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix lingua: <http://www.w3.org/2000/10/swap/lingua#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix : <http://example.org/ns#>.

:gen_rule a lingua:ForwardRule;
    lingua:premise ();
    lingua:conclusion (
        :Socrates a :Human
        :Human rdfs:subClassOf :Mortal
        :rdfs_subclass_rule a lingua:ForwardRule
        :rdfs_subclass_rule lingua:premise (
            var:A rdfs:subClassOf var:B
            var:S a var:A
        )
        :rdfs_subclass_rule lingua:conclusion (
            var:S a var:B
        )
        :socrates_query a lingua:QueryRule
        :socrates_query lingua:premise (
            var:S a :Mortal
        )
        :socrates_query lingua:conclusion (
            var:S a :Mortal
        )
    ).
