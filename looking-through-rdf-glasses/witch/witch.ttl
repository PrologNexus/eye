@prefix rule: <http://eyereasoner.github.io/rule#>.
@prefix : <urn:example:>.

# \forall x : BURNS(x) /\ WOMAN(x) => WITCH(x)
[] a rule:ForwardRule;
    rule:premise (
        (_:x a :BURNS)
        (_:x a :WOMAN)
    );
    rule:conclusion (
        (_:x a :WITCH)
    ).

# WOMAN(GIRL)
:GIRL a :WOMAN.

# \forall x : ISMADEOFWOOD(x) => BURNS(x)
[] a rule:ForwardRule;
    rule:premise (
        (_:x a :ISMADEOFWOOD)
    );
    rule:conclusion (
        (_:x a :BURNS)
    ).

# \forall x : FLOATS(x) => ISMADEOFWOOD(x)
[] a rule:ForwardRule;
    rule:premise (
        (_:x a :FLOATS)
    );
    rule:conclusion (
        (_:x a :ISMADEOFWOOD)
    ).

# FLOATS(DUCK)
:DUCK a :FLOATS.

# \forall x,y : FLOATS(x) /\ SAMEWEIGHT(x,y) => FLOATS(y)
[] a rule:ForwardRule;
    rule:premise (
        (_:x a :FLOATS)
        (_:x :SAMEWEIGHT _:y)
    );
    rule:conclusion (
        (_:y a :FLOATS)
    ).

# and, by experiment
# SAMEWEIGHT(DUCK,GIRL)
:DUCK :SAMEWEIGHT :GIRL.

# who's a witch?
[] a rule:QueryRule;
    rule:premise (
        (_:S a :WITCH)
    );
    rule:conclusion (
        (_:S a :WITCH)
    ).
